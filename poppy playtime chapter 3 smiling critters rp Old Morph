local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/RedzLibV5/main/Source.Lua"))()

local Window = redzlib:MakeWindow({
  Title = "poppy playtime chapter 3 smiling critters rp Old Morph,
  SubTitle = "Welcome to Script hooaczx & 1hooaczx1 â€¢ TH",
  SaveFolder = "Redz Config"
})

local Tab1 = Window:MakeTab({"Old Morph", "cool"})

Tab1:AddButton({"Cartoon DogDayV1", function()
local ohInstance1 = game:GetService("ReplicatedStorage").Monsters["Cartoon DogDay"]

game:GetService("ReplicatedStorage").CharChange:FireServer(ohInstance1)
end})

Tab1:AddButton({"Cartoon CatnapV1", function(
local ohInstance1 = game:GetService("ReplicatedStorage").Monsters["Catnap Cartoon"]

game:GetService("ReplicatedStorage").CharChange:FireServer(ohInstance1)
end})
