        for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
            track:Stop()
        end
    end

    if animate:FindFirstChild("jump") then
        animate:WaitForChild("jump"):FindFirstChildOfClass("Animation").AnimationId = "http://www.roblox.com/asset/?id=0"
    end
end

