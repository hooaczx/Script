    local character = player.Character
    local animate = character:WaitForChild("Animate")

    if animate:FindFirstChild("jump") then
        animate:WaitForChild("jump"):FindFirstChildOfClass("Animation").AnimationId = "http://www.roblox.com/asset/?id=4708188025"
    end
