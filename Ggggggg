local Image = AddImageLabel(Main1, {
  Name = "Cowboy",
  Image = "rbxthumb://type=Asset&id=11285370338&w=420&h=420"
})
AddButton(Main1, {
  Name = "Changer",
  Callback = function()
  local player = game.Players.LocalPlayer

if player.Character and player.Character:FindFirstChildOfClass("Humanoid").RigType == Enum.HumanoidRigType.R6 then
    return
end

local function AntiGoofy()
    local character = player.Character
    local animate = character:WaitForChild("Animate")

    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
            track:Stop()
        end
    end

    if animate:FindFirstChild("run") then
        animate:WaitForChild("run"):FindFirstChildOfClass("Animation").AnimationId = "http://www.roblox.com/asset/?id=0"
    end
end

local function setAnimations()
    local character = player.Character
    local animate = character:WaitForChild("Animate")

    if animate:FindFirstChild("run") then
        animate:WaitForChild("run"):FindFirstChildOfClass("Animation").AnimationId = "http://www.roblox.com/asset/?id=1014401683"
    end
end

local function onCharacterAdded(character)
    AntiGoofy()
    setAnimations()
end

player.CharacterAdded:Connect(onCharacterAdded)


if player.Character then
    onCharacterAdded(player.Character)
end
local NotificationLoad = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hooaczx/Script/main/notification%20ui%20library%20by%20treeofplant'),true))()

NotificationLoad:NewNotification({
    ["Mode"] = "Custom",
    ["Title"] = "Animation",
    ["Description"] = "Success",
    ["Notification_Image"] = "rbxthumb://type=Asset&id=11285370338&w=420&h=420",
    ["Notification_Sound"] = "rbxassetid://570433034",
    ["Timeout"] = 1,
    ["Audio"] = true
})
  end
})
