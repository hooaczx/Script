loadstring(game:HttpGet(("https://raw.githubusercontent.com/hooaczx/Script/main/REDzHUB%20LibraryV2")))()

MakeWindow({
  Hub = {
    Title = "Animation Changer",
    Animation = "By : hooaczx & 1hooaczx1"
  },
  Key = {
    KeySystem = false,
    Title = "Key System",
    Description = "",
    KeyLink = "",
    Keys = {"1234"},
    Notifi = {
      Notifications = true,
      CorrectKey = "Running the Script...",
      Incorrectkey = "The key is incorrect",
      CopyKeyLink = "Copied to Clipboard"
    }
  }
})


MinimizeButton({
  Image = "rbxthumb://type=Asset&id=18488884411&w=150&h=150",
  Size = {40, 40},
  Color = Color3.fromRGB(10, 10, 10),
  Corner = true,
  Stroke = false,
  StrokeColor = Color3.fromRGB(255, 0, 0)
})
MakeNotifi({
  Title = "Hi",
  Text = "I want to say that I hope you Enjoy My script",
  Time = 5
})
local Main1 = MakeTab({Name = "idle"})
local Image = AddImageLabel(Main1, {
  Name = "Default Current",
  Image = "rbxthumb://type=BundleThumbnail&id=507766666&w=420&h=420"
})
AddButton(Main1, {
  Name = "Changer",
  Callback = function()
  local player = game.Players.LocalPlayer

if player.Character and player.Character:FindFirstChildOfClass("Humanoid").RigType == Enum.HumanoidRigType.R6 then
    return
end

local function AntiGoofy()
    local character = player.Character
    local animate = character:WaitForChild("Animate")

    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
            track:Stop()
        end
    end

    if animate:FindFirstChild("idle") then
        animate.idle:WaitForChild("Animation1").AnimationId = "http://www.roblox.com/asset/?id=0"
        animate.idle:WaitForChild("Animation2").AnimationId = "http://www.roblox.com/asset/?id=0"
    end
end

local function setAnimations()
    local character = player.Character
    local animate = character:WaitForChild("Animate")

    if animate:FindFirstChild("idle") then
        animate.idle:WaitForChild("Animation1").AnimationId = "http://www.roblox.com/asset/?id=507766388"
        animate.idle:WaitForChild("Animation2").AnimationId = "http://www.roblox.com/asset/?id=507766666"
    end
end

local function onCharacterAdded(character)
    AntiGoofy()
    setAnimations()
end

player.CharacterAdded:Connect(onCharacterAdded)


if player.Character then
    onCharacterAdded(player.Character)
end
local NotificationLoad = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hooaczx/Script/main/notification%20ui%20library%20by%20treeofplant'),true))()

NotificationLoad:NewNotification({
    ["Mode"] = "Custom",
    ["Title"] = "Animation",
    ["Description"] = "Success",
    ["Notification_Image"] = "rbxthumb://type=Asset&id=507766666&w=150&h=150",
    ["Notification_Sound"] = "rbxassetid://570433034",
    ["Timeout"] = 1,
    ["Audio"] = true
})
  end
})
