        animate.idle:WaitForChild("Animation1").AnimationId = "http://www.roblox.com/asset/?id=1069977950"
        animate.idle:WaitForChild("Animation2").AnimationId = "http://www.roblox.com/asset/?id=1069987858"
    end
end

