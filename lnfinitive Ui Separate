	local script = Instance.new('Script', skybox)

	script.Parent.MouseButton1Click:Connect(function()
		local sky = Instance.new("Sky", game.Lighting)
		sky.SkyboxBk = "http://www.roblox.com/asset/?id=17876090456"
sky.SkyboxFt = "http://www.roblox.com/asset/?id=17876090456"
sky.SkyboxLf = "http://www.roblox.com/asset/?id=17876090456"
sky.SkyboxRt = "http://www.roblox.com/asset/?id=17876090456"
sky.SkyboxUp = "http://www.roblox.com/asset/?id=17876090456"
sky.SkyboxDn = "http://www.roblox.com/asset/?id=17876090456"

	end)
end
