Tab:CreateButton("Fe laggy guy (By Jinxx",function()
-- script made by jinxx and not by chatgpt fuckers
local H = Instance.new("Hint", game.Workspace)
H.Parent = game.Workspace
H.Text = ("L")
wait(.1)
H.Text = ("La")
