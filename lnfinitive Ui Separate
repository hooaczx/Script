			l.FogEnd = math.random(50, 100)
			l.FogStart = l.FogEnd
			l.TimeOfDay = "0:00:00"
			l.Brightness = -1000000000000
			l.Ambient = Color3.new(255, 0, 0)
			l.OutdoorAmbient = Color3.new(math.random(), math.random(), math.random())
		end
	end)
end
