Tab:CreateButton("speed demonstrator",function()
-- ServerScript veya LocalScript içinde

-- Oyuncunun GUI'sini bulun
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- ScreenGui oluşturun
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "SpeedGui"  -- GUI'nin adını ayarlayın
screenGui.Parent = playerGui -- GUI'yi PlayerGui'nin altına ekleyin

-- TextLabel oluşturun
local textLabel = Instance.new("TextLabel")
textLabel.Name = "SpeedLabel"   -- Label'in adını ayarlayın
textLabel.Size = UDim2.new(0, 200, 0, 50)   -- Boyutunu ayarlayın
textLabel.Position = UDim2.new(0, 10, 0, 10)   -- Konumunu ayarlayın
textLabel.Text = "Hız: 0"  -- Başlangıçta gösterilecek metin
textLabel.TextSize = 24   -- Metin boyutunu ayarlayın
textLabel.BackgroundTransparency = 0.5   -- Arka planın yarı saydamlığını ayarlayın
textLabel.Parent = screenGui   -- Label'i ScreenGui'nin altına ekleyin

-- Hızı güncelleyen fonksiyon
local function updateSpeed()
    while true do
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                local speed = humanoid.WalkSpeed
                textLabel.Text = "Hız: " .. tostring(speed)
            end
        end
        wait(0.1) -- Her 0.1 saniyede bir günceller
    end
end

-- Oyuncu karakteri yüklendiğinde hızı güncelle
player.CharacterAdded:Connect(function(character)
    updateSpeed()
end)

-- İlk karakter yüklendiyse hemen başlat
if player.Character then
    updateSpeed()
end
end) 

