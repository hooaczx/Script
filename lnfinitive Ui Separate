local function DJSVTZU_fake_script() -- SetSkybox.Script 
	local script = Instance.new('Script', SetSkybox)

	function click()
		s = Instance.new("Sky")
		s.Name = "Sky"
		s.Parent = game.Lighting
		s.SkyboxBk = "http://www.roblox.com/asset/?id=13774730449"
		s.SkyboxDn = "http://www.roblox.com/asset/?id=13774730449"
		s.SkyboxFt = "http://www.roblox.com/asset/?id=13774730449"
		s.SkyboxLf = "http://www.roblox.com/asset/?id=13774730449"
		s.SkyboxRt = "http://www.roblox.com/asset/?id=13774730449"
		s.SkyboxUp = "http://www.roblox.com/asset/?id=13774730449"
		game.Lighting.TimeOfDay = 12
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(DJSVTZU_fake_script)()
local function PMIW_fake_script() -- DecalSpam.Script 
	local script = Instance.new('Script', DecalSpam)

	function click()
		function exPro(root)
			for _, v in pairs(root:GetChildren()) do
				if v:IsA("Decal") and v.Texture ~= "http://www.roblox.com/asset/?id=13774730449" then
					v.Parent = nil
				elseif v:IsA("BasePart") then
					v.Material = "Plastic"
					v.Transparency = 0
					local One = Instance.new("Decal", v)
					local Two = Instance.new("Decal", v)
					local Three = Instance.new("Decal", v)
					local Four = Instance.new("Decal", v)
					local Five = Instance.new("Deca", v)
					local Six = Instance.new("Decal", v)
					One.Texture = "http://www.roblox.com/asset/?id=13774730449"
					Two.Texture = "http://www.roblox.com/asset/?id=13774730449"
					Three.Texture = "http://www.roblox.com/asset/?id=13774730449"
					Four.Texture = "http://www.roblox.com/asset/?id=13774730449"
					Five.Texture = "http://www.roblox.com/asset/?id=13774730449"
					Six.Texture = "http://www.roblox.com/asset/?id=13774730449"
					One.Face = "Front"
					Two.Face = "Back"
					Three.Face = "Right"
					Four.Face = "Left"
					Five.Face = "Top"
					Six.Face = "Bottom"
				end
				exPro(v)
			end
		end
		function asdf(root)
			for _, v in pairs(root:GetChildren()) do
				asdf(v)
			end
		end
		exPro(game.Workspace)
		asdf(game.Workspace)
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(PMIW_fake_script)()
