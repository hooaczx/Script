local function SendDetailedNotification(title, text, duration)
    game.StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration or 5
    })
end

local currentGameId = game.PlaceId

AddToggle(Main5u6, {
  Name = "Infinite Jump",
  Default = false,
  Callback = function(s)
  getgenv().InfJ = s
    game:GetService("UserInputService").JumpRequest:connect(function()
        if InfJ == true then
            game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
        end
    end)
  end
})

function resetHighlight()
    if highlightObject then
        highlightObject:Destroy()
        highlightObject = nil
    end
    if characterAddedConnection then
        characterAddedConnection:Disconnect()
        characterAddedConnection = nil
    end
    highlightedPlayer = nil
end

function lookAtClosestPlayer()
    local character = lp.Character or lp.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    local humanoid = character:WaitForChild("Humanoid")
    local closestPlayer = nil
    local closestDistance = detectionRadius
    local closestVerticalDifference = math.huge

    for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
        if otherPlayer ~= lp and otherPlayer.Character then
            local otherHumanoidRootPart = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
            if otherHumanoidRootPart then
                local horizontalDistance = (Vector3.new(humanoidRootPart.Position.X, 0, humanoidRootPart.Position.Z) -
                    Vector3.new(otherHumanoidRootPart.Position.X, 0, otherHumanoidRootPart.Position.Z)).Magnitude
                local verticalDifference = math.abs(humanoidRootPart.Position.Y - otherHumanoidRootPart.Position.Y)

                if horizontalDistance < closestDistance or
                   (horizontalDistance == closestDistance and verticalDifference < closestVerticalDifference) then
                    closestPlayer = otherPlayer
                    closestDistance = horizontalDistance
                    closestVerticalDifference = verticalDifference
                end
            end
        end
    end

    if closestPlayer then
        if closestPlayer ~= highlightedPlayer then
            resetHighlight()
            highlightedPlayer = closestPlayer

            highlightObject = Instance.new("Highlight")
            highlightObject.Adornee = closestPlayer.Character
            highlightObject.FillColor = Color3.fromRGB(255, 255, 0)
            highlightObject.OutlineColor = Color3.fromRGB(255, 255, 0)
            highlightObject.FillTransparency = 1
            highlightObject.OutlineTransparency = 0
            highlightObject.Parent = closestPlayer.Character

            characterAddedConnection = closestPlayer.CharacterAdded:Connect(function()
                resetHighlight()
                highlightObject = Instance.new("Highlight")
                highlightObject.Adornee = closestPlayer.Character
                highlightObject.FillColor = Color3.fromRGB(255, 255, 0)
                highlightObject.OutlineColor = Color3.fromRGB(255, 255, 0)
                highlightObject.FillTransparency = 1
                highlightObject.OutlineTransparency = 0
                highlightObject.Parent = closestPlayer.Character
            end)
        end

        local targetPosition = closestPlayer.Character.HumanoidRootPart.Position
        local horizontalTargetPosition = Vector3.new(targetPosition.X, humanoidRootPart.Position.Y, targetPosition.Z)
        local isWalking = humanoid.MoveDirection.Magnitude > 0
        if isWalking then
            humanoidRootPart.CFrame = CFrame.lookAt(humanoidRootPart.Position, horizontalTargetPosition)
        else
            humanoidRootPart.CFrame = humanoidRootPart.CFrame:Lerp(
                CFrame.lookAt(humanoidRootPart.Position, horizontalTargetPosition), 0.3
            )
        end
    else
        resetHighlight()
    end
end

AddTextBox(Main5u6, {
    Name = "Detection Radius",
    Default = "Enter",
    PlaceholderText = "Enter number",
    ClearText = true,
    Callback = function(value)
        local newValue = tonumber(value)
        if newValue and newValue > 0 then
            detectionRadius = newValue
        end
    end
})

AddToggle(Main5u6, {
    Name = "LookAt Player",
    Default = false,
    Callback = function(state)
        if state then
            lookAtconnection = RunService.Heartbeat:Connect(lookAtClosestPlayer)
            characterAddedConnection = lp.CharacterAdded:Connect(function()
                if lookAtconnection then
                    lookAtconnection:Disconnect()
                end
                lookAtconnection = RunService.Heartbeat:Connect(lookAtClosestPlayer)
            end)
        else
            if lookAtconnection then
                lookAtconnection:Disconnect()
                lookAtconnection = nil
            end
            if characterAddedConnection then
                characterAddedConnection:Disconnect()
                characterAddedConnection = nil
            end
            if highlightObject then
                highlightObject:Destroy()
                highlightObject = nil
            end
            highlightedPlayer = nil
            local character = lp.Character
            if character then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    humanoidRootPart.CFrame = CFrame.new(humanoidRootPart.Position)
                end
            end
        end
    end
})

