local _c___, _p___, _r___, _loop___

AddToggle(Main5u6, {
    Name = "ClickTp",
    Default = false,
    Callback = function(_s___)
        if _s___ then
            game:GetService("UserInputService").InputBegan:Connect(function(_i___)
                if _i___.UserInputType == Enum.UserInputType.Touch or _i___.UserInputType == Enum.UserInputType.MouseButton1 then
                    _p___ = _i___.Position
                end
            end)
            _c___ = game:GetService("UserInputService").InputEnded:Connect(function(_i___, _g___)
                if _g___ or not _p___ or (_i___.Position - _p___).Magnitude > 5 then return end
                if _i___.UserInputType ~= Enum.UserInputType.Touch and _i___.UserInputType ~= Enum.UserInputType.MouseButton1 then return end
                local _m___ = game:FindFirstChildOfClass("Players").LocalPlayer:GetMouse()
                if _m___.Target and _m___.Target.Transparency < 1 then
                    local _p2___ = _m___.Hit.p
                    local _c2___ = game:FindFirstChildOfClass("Players").LocalPlayer.Character or game:FindFirstChildOfClass("Players").LocalPlayer.CharacterAdded:Wait()
                    local _h2___ = _c2___:WaitForChild("HumanoidRootPart")
                    _h2___.CFrame = CFrame.new(_p2___)
                    if _r___ then
                        _r___.CFrame = CFrame.new(_p2___)
                    end
                end
            end)
            _loop___ = game:GetService("RunService").Heartbeat:Connect(function()
                if not _r___ or not workspace:FindFirstChild("ClickTP_Marker_hooaczx") then
                    if _r___ then
                        _r___:Destroy()
                    end 
                    _r___ = Instance.new("Part")
                    _r___.Anchored = true
                    _r___.CanCollide = false
                    _r___.Shape = Enum.PartType.Ball
                    _r___.Size = Vector3.new(0.5, 0.5, 0.5)
                    _r___.Color = Color3.fromRGB(0, 255, 0)
                    _r___.Material = Enum.Material.ForceField
                    _r___.Transparency = 0.2
                    _r___.CFrame = CFrame.new(Vector3.new(0, 0, 0))
                    _r___.Name = "ClickTP_Marker_hooaczx"
                    _r___.Parent = workspace
                end
            end)
        elseif _c___ then
            _c___:Disconnect()
            _c___ = nil
            if _loop___ then _loop___:Disconnect() _loop___ = nil end
            if _r___ then _r___:Destroy() _r___ = nil end
        end
    end
})

local x1 = false
local x2
local x3

AddToggle(Main5u6, {
    Name = "Touch Fling",
    Default = false,
    Callback = function(x4)
        x1 = x4
        if x1 then
          if not x2 then
            x2 = RunService.Heartbeat:Connect(function()
                local x5 = game:FindFirstChildOfClass("Players").LocalPlayer
                local x6, x7, x8, x9 = nil, nil, nil, 0.1
                x6 = x5.Character
                x7 = x6 and x6:FindFirstChild("HumanoidRootPart")
                if x7 then
                    x8 = x7.Velocity
                    x7.Velocity = x8 * 10000 + Vector3.new(0, 10000, 0)
                    RunService.RenderStepped:Wait()
                    x7.Velocity = x8
                    RunService.Stepped:Wait()
                    x7.Velocity = x8 + Vector3.new(0, x9, 0)
                    x9 = -x9
                 end
              end)
           end
        else
            if x2 then
                x2:Disconnect()
                x2 = nil
            end
        end
    end
})

local Anti_teleport_server = loadstring(game:HttpGet("https://raw.githubusercontent.com/hooaczx/Script/refs/heads/main/Anti%20Teleport%20Server"))()

