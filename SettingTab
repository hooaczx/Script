AddTextBox(Main5u6, {
	Name = "Number Tpwalk",
	Default = "Enter",
	PlaceholderText = "Enter number",
	ClearText = true,
	Callback = function(v)
		local n = tonumber(v)
		if n then getgenv().TPSpeed = n end
	end
})

AddToggle(Main5u6, {
	Name = "Tpwalk",
	Default = false,
	Callback = function(state)
		getgenv().TPWalk = state
		if state then
			if not tpc then
				local rs = game:GetService("RunService")
				local plr = game.Players.LocalPlayer
				tpc = rs.Heartbeat:Connect(function()
					local ch = plr.Character
					local hum = ch and ch:FindFirstChildOfClass("Humanoid")
					if not ch or not hum or not ch.Parent or not getgenv().TPWalk then return end
					local spd = tonumber(getgenv().TPSpeed) or 1
					if hum.MoveDirection.Magnitude > 0 then
						ch:TranslateBy(hum.MoveDirection * spd)
					end
				end)
			end
		else
			if tpc then
				tpc:Disconnect()
				tpc = nil
			end
		end
	end
})

