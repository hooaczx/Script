    Name = "Esp Player Color",
    Default = Color3.fromRGB(0, 0, 255),
    Callback = function(color)
        _G.ESPColor = color
        if _G.ESP then
            for _, highlight in ipairs(Storage:GetChildren()) do
                highlight.FillColor = color
                highlight.OutlineColor = color
            end
        end
    end
})

