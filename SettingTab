local Main5u6 = MakeTab({Name = "Setting"})
local function isNumber(str)
    return tonumber(str) ~= nil or str == 'inf'
end
local camera = workspace.CurrentCamera
local _zx_dWS, _zx_dJP = 16, 50
local _zx_ws, _zx_jp
getgenv().TPSpeed = 0
getgenv().TPJpBst = 0
getgenv().TPWalk = false
getgenv().Cframewalk = 0
getgenv().NV_Brightness = 0
local player = game:FindFirstChildOfClass("Players").LocalPlayer
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local LocalPlayer = player
local players = game:FindFirstChildOfClass('Players')
local lplr = players.LocalPlayer
local ps = game:FindFirstChildOfClass("Players")
local lp = ps.LocalPlayer
local mouse = game:FindFirstChildOfClass("Players").LocalPlayer:GetMouse()
local gui, con, isOn, fpsV = nil, nil, false, 0
local tpc, cfc = nil, nil
local cfMode, Opt = "None", "Nope"
local TE = false

Service = setmetatable({}, {
    __index = function(Self, Index)
        local NewService
        pcall(function()
            NewService = game:GetService(Index)
        end)
        if not NewService and Index == "Players" then
            NewService = game:FindFirstChildOfClass("Players")
        end
        if NewService then
            Self[Index] = NewService
        end
        return NewService
    end
})

local localPuppet = Service.Players and Service.Players.LocalPlayer

function SendDetailedNotification(title, text, duration)
    game.StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration or 5
    })
end

function Create(instance, parent, props)
    local new = Instance.new(instance, parent)
    if props then
        for prop, value in pairs(props) do
            new[prop] = value
        end
    end
    return new
end

function CreateTween(instance, prop, value, time, tweenWait)
    local tween = TweenService:Create(instance, TweenInfo.new(time, Enum.EasingStyle.Linear), {[prop] = value})
    tween:Play()
    if tweenWait then
        tween.Completed:Wait()
    end
end

if game.PlaceId == 7117492592 then
    --Support Map Realistic Hand RP ðŸ–ðŸ¤ Without VR
    local wHk8_zB = {}
    local pVj2_rT = nil
    AddToggle(Main5u6, { 
        Name = "Afk in Public Map[Realistic Hand RP ðŸ–ðŸ¤ Without VR](Anti Touching and Abilities by Hand)",
        Default = false,
        Callback = function(dFq9_xL)
            if dFq9_xL then
                if pVj2_rT then return end
                pVj2_rT = game:GetService("RunService").Heartbeat:Connect(function()
                    local aNm3_cS = {}
                    for _, uLk7_vQ in ipairs(game:GetService("Players"):GetPlayers()) do
                        aNm3_cS[uLk7_vQ] = true
                        if not wHk8_zB[uLk7_vQ] then
                            game:GetService("ReplicatedStorage").Remotes.PermissionsRequestBlock:FireServer("Touching", uLk7_vQ, true)
                            game:GetService("ReplicatedStorage").Remotes.PermissionsRequestBlock:FireServer("Abilities", uLk7_vQ, true)
                            wHk8_zB[uLk7_vQ] = true
                        end
                    end
                    for vQs1_yM in pairs(wHk8_zB) do
                        if not aNm3_cS[vQs1_yM] then
                            wHk8_zB[vQs1_yM] = nil
                        end
                    end
                end)
            else
                if pVj2_rT then
                    pVj2_rT:Disconnect()
                    pVj2_rT = nil
                end
                for _, uLk7_vQ in ipairs(game:GetService("Players"):GetPlayers()) do
                    game:GetService("ReplicatedStorage").Remotes.PermissionsRequestBlock:FireServer("Touching", uLk7_vQ, false)
                    game:GetService("ReplicatedStorage").Remotes.PermissionsRequestBlock:FireServer("Abilities", uLk7_vQ, false)
                end
                wHk8_zB = {}
            end
        end
    })
elseif game.PlaceId == 89490917156284 or game.PlaceId == 97464845197302 then
    --Support Map Nightmare Run
    AddToggle(Main5u6,{
      Name="Auto Pick Up Cheeses For Event Monster Rat[Nightmare Run Game]",
      Default=false,
      Callback=function(v)
           if v then
              get = game:GetService("RunService").Heartbeat:Connect(function()
                  local p = game:GetService("Players").LocalPlayer
                if not p then return end
                local ch = p.Character or p.CharacterAdded:Wait()
                local hrp = ch and ch:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                local cheesess = workspace:FindFirstChild("Cheeses")
                if not cheesess then return end
                local cheeses = cheesess:GetChildren()
                if #cheeses == 0 then return end
                for _, cheese in ipairs(cheeses) do
                    if not v then return end
                    if cheese and cheese:IsDescendantOf(workspace) then
                        local pos
                        if cheese:IsA("BasePart") then
                            pos = cheese.CFrame
                        elseif cheese:IsA("Model") and cheese.PrimaryPart then
                            pos = cheese.PrimaryPart.CFrame
                        end
                        if pos then
                            hrp.CFrame = pos
                            repeat task.wait(0.1) until not cheese or not cheese:IsDescendantOf(workspace)
                            task.wait(0.2)
                        end
                    end
                end
            end)
            else
               if get then
                  get:Disconnect()
                  get=nil
               end
           end
       end
    })

    local hTable = {}
    local vZq7_mK = nil
    AddToggle(Main5u6, {
        Name = "ESP Monsters[Nightmare Run Game]",
        Default = false,
        Callback = function(fGj8_tP)
            if fGj8_tP then
                if vZq7_mK then return end
                vZq7_mK = game:GetService("RunService").Heartbeat:Connect(function()
                    local mt = workspace:FindFirstChild("Monsters")
                    if not mt then return end
                    local now = {}
                    for _, mon in ipairs(mt:GetChildren()) do
                        now[mon] = true
                        if not hTable[mon] or not hTable[mon].Parent then
                            local hl = Instance.new("Highlight")
                            hl.Name = "highlight_monster_"..mon.Name
                            hl.Adornee = mon
                            hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                            hl.OutlineTransparency = 0
                            hl.OutlineColor = Color3.new(1,1,1)
                            hl.FillColor = Color3.new(1,1,1)
                            hl.Parent = mon
                            game:GetService("TweenService"):Create(
                                hl,
                                TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true),
                                { FillColor = Color3.new(0,0,0) }
                            ):Play()
                            hTable[mon] = hl
                        end
                    end
                    for mon in pairs(hTable) do
                        if not now[mon] or not mon:IsDescendantOf(workspace) then
                            if hTable[mon] and hTable[mon].Parent then
                                hTable[mon]:Destroy()
                            end
                            hTable[mon] = nil
                        end
                    end
                end)
            else
                if vZq7_mK then
                    vZq7_mK:Disconnect()
                    vZq7_mK = nil
                end
                for mon, hl in pairs(hTable) do
                    if hl and hl.Parent then
                        hl:Destroy()
                        hTable[mon] = nil
                    end
                end
                hTable = {}
            end
        end
    })
end

