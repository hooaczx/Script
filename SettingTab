    Default = false,
    Callback = function(s_1)
        Anti_teleport_server.Toggle(s_1)
    end
})

local _Zxa, _Zxb, _Zxc = nil, setmetatable({}, { __mode = "k" }), false
local _Zxd, _Zxe, _Zxf, _Zxg, _Zxh, _Zxi, _Zxq, _Zxk = {}, {}, {}, {}, {}, {}, {}, {}

AddToggle(Main5u6, {
  Name = "Anti Fling",
  Default = false,
  Callback = function(_Zxs)
    if _Zxs then
      if _Zxc then return end
      _Zxc = true
      if not _Zxa then
        _Zxa = game:GetService("RunService").RenderStepped:Connect(function()
          local _Zxpls = game:FindFirstChildOfClass("Players"):GetPlayers()
          if #_Zxpls <= 1 then return end
          local _Zxpresent = {}
          for _Zxu = 1, #_Zxpls do _Zxpresent[_Zxpls[_Zxu]] = true end
          for _Zxr in pairs(_Zxe) do
            if not _Zxpresent[_Zxr] then
              local _Zxl = _Zxd[_Zxr]
              if _Zxl then
                for _Zxv = 1, #_Zxl do
                  local _Zxw = _Zxl[_Zxv]
                  if _Zxw and _Zxw.Parent and _Zxw:IsA("BasePart") then
                    _Zxw.CanCollide = _Zxb[_Zxw]
                  end
                  _Zxb[_Zxw] = nil
                end
              end
              _Zxd[_Zxr] = nil; _Zxe[_Zxr] = nil; _Zxf[_Zxr] = nil
              _Zxg[_Zxr] = nil; _Zxh[_Zxr] = nil; _Zxi[_Zxr] = nil; _Zxq[_Zxr] = nil; _Zxk[_Zxr] = nil
            end
          end
          if not _Zxq.index or type(_Zxq.index) ~= "number" then _Zxq.index = 1 end
          local _Zcount = #_Zxpls
          if _Zcount == 0 then return end
          if _Zxq.index > _Zcount then _Zxq.index = 1 end
          local _Zp = _Zxpls[_Zxq.index]
          if _Zp == game:GetService("Players").LocalPlayer then
            _Zxq.index = _Zxq.index + 1
            if _Zxq.index > _Zcount then _Zxq.index = 1 end
            return
          end
          local _Zch = _Zp.Character
          if not _Zch or _Zch.Parent ~= workspace then
            if _Zxe[_Zp] then
              local _Zparts = _Zxd[_Zp]
              if _Zparts then
                for _Zxv = 1, #_Zparts do
                  local _Zpart = _Zparts[_Zxv]
                  if _Zpart and _Zpart.Parent and _Zpart:IsA("BasePart") then
                    _Zpart.CanCollide = _Zxb[_Zpart]
                  end
                  _Zxb[_Zpart] = nil
                end
              end
              _Zxd[_Zp] = nil; _Zxe[_Zp] = nil; _Zxf[_Zp] = nil
              _Zxg[_Zp] = nil; _Zxh[_Zp] = nil; _Zxi[_Zp] = nil; _Zxq[_Zp] = nil; _Zxk[_Zp] = nil
            end
            _Zxq.index = _Zxq.index + 1
            if _Zxq.index > _Zcount then _Zxq.index = 1 end
            return
          end
          local _Zhum = _Zch:FindFirstChildWhichIsA("Humanoid")
          if not _Zhum then
            if _Zxe[_Zp] then
              local _Zparts = _Zxd[_Zp]
              if _Zparts then
                for _Zxv = 1, #_Zparts do
                  local _Zpart = _Zparts[_Zxv]
                  if _Zpart and _Zpart.Parent and _Zpart:IsA("BasePart") then
                    _Zpart.CanCollide = _Zxb[_Zpart]
                  end
                  _Zxb[_Zpart] = nil
                end
              end
              _Zxd[_Zp] = nil; _Zxe[_Zp] = nil; _Zxf[_Zp] = nil
              _Zxg[_Zp] = nil; _Zxh[_Zp] = nil; _Zxi[_Zp] = nil; _Zxq[_Zp] = nil; _Zxk[_Zp] = nil
            end
            _Zxq.index = _Zxq.index + 1
            if _Zxq.index > _Zcount then _Zxq.index = 1 end
            return
          end
          local _Zprev = _Zxg[_Zp]
          local _Zcur = _Zhum.Health
          if _Zprev and _Zprev <= 0 and _Zcur > 0 then
            _Zxd[_Zp] = nil; _Zxe[_Zp] = nil; _Zxf[_Zp] = nil
            _Zxg[_Zp] = nil; _Zxh[_Zp] = nil; _Zxi[_Zp] = nil; _Zxq[_Zp] = nil; _Zxk[_Zp] = nil
          end
          _Zxg[_Zp] = _Zcur
          local _Zneed = (_Zxe[_Zp] ~= _Zch) or (_Zxf[_Zp] ~= _Zhum) or (_Zxd[_Zp] == nil)
          if _Zneed then
            _Zxe[_Zp] = _Zch
            _Zxf[_Zp] = _Zhum
            local _Znew = {}
            local _Zdesc = _Zch:GetDescendants()
            for _Zv = 1, #_Zdesc do
              local _Zpart = _Zdesc[_Zv]
              if _Zpart and _Zpart:IsA("BasePart") then
                _Znew[#_Znew + 1] = _Zpart
                if _Zxb[_Zpart] == nil then
                  _Zxb[_Zpart] = _Zpart.CanCollide
                end
                if _Zxb[_Zpart] == true and _Zpart.CanCollide == true then
                  _Zpart.CanCollide = false
                end
              end
            end
            _Zxd[_Zp] = _Znew
          else
            local _Zparts = _Zxd[_Zp]
            if _Zparts then
              local _Zni = 1
              for _Zv = 1, #_Zparts do
                local _Zpart = _Zparts[_Zv]
                if _Zpart and _Zpart.Parent and _Zpart:IsA("BasePart") then
                  if _Zxb[_Zpart] == true and _Zpart.CanCollide == true then
                    _Zpart.CanCollide = false
                  end
                  _Zparts[_Zni] = _Zpart
                  _Zni = _Zni + 1
                else
                  _Zxb[_Zpart] = nil
                end
              end
              for _Zv = _Zni, #_Zparts do _Zparts[_Zv] = nil end
            end
          end
          local _Zall = true
          local _Zcheck = _Zxd[_Zp]
          if _Zcheck then
            for _Zv = 1, #_Zcheck do
              local _Zpt = _Zcheck[_Zv]
              if _Zpt and _Zpt.Parent and _Zpt:IsA("BasePart") then
                if _Zxb[_Zpt] == true then
                  if _Zpt.CanCollide == true then
                    _Zall = false
                    _Zpt.CanCollide = false
                  end
                end
              else
                _Zall = false
              end
            end
          else
            _Zall = false
          end
          if _Zall then
            _Zxk[_Zp] = true
            _Zxq.index = _Zxq.index + 1
            if _Zxq.index > _Zcount then _Zxq.index = 1 end
          else
            _Zxk[_Zp] = false
          end
        end)
      end
    else
      if _Zxa then _Zxa:Disconnect() _Zxa = nil end
      for _Zp, _Zxparts in pairs(_Zxd) do
        if _Zxparts then
          for _Zv = 1, #_Zxparts do
            local _Zpt = _Zxparts[_Zv]
            if _Zpt and _Zpt.Parent and _Zpt:IsA("BasePart") then
              _Zpt.CanCollide = _Zxb[_Zpt]
            end
            _Zxb[_Zpt] = nil
          end
        end
      end
      for _Zu in pairs(_Zxd) do _Zxd[_Zu] = nil end
      for _Zu in pairs(_Zxe) do _Zxe[_Zu] = nil end
      for _Zu in pairs(_Zxf) do _Zxf[_Zu] = nil end
      for _Zu in pairs(_Zxg) do _Zxg[_Zu] = nil end
      for _Zu in pairs(_Zxh) do _Zxh[_Zu] = nil end
      for _Zu in pairs(_Zxi) do _Zxi[_Zu] = nil end
      for _Zu in pairs(_Zxq) do _Zxq[_Zu] = nil end
      for _Zu in pairs(_Zxb) do _Zxb[_Zu] = nil end
      _Zxc = false
    end
  end
})

if game.PlaceId == 16116270224 or game.PlaceId == 16552821455 or game.PlaceId == 18984416148 or game.PlaceId == 113160965141072 then
    AddWarningMessage(Main5u6, {
        WarningMessage = "Noclip, Removed due to security concerns, may be banned from the game",
        ImageId = "rbxthumb://type=Asset&id=116573046454042&w=420&h=420"
    })
else
    local ncp, ncpDflt = {}, nil
    AddToggle(Main5u6, {
        Name = "No-Clip",
        Default = false,
        Callback = function(state)
            if state then
                if not ncpDflt then 
                    ncpDflt = game:GetService("RunService").Stepped:Connect(function()
                        local character = game:FindFirstChildOfClass("Players").LocalPlayer.Character
                        if character then
                            for _, part in ipairs(character:GetDescendants()) do
                                if part:IsA("BasePart") then
                                    if not ncp[part] then
                                        ncp[part] = part.CanCollide
                                    end
                                    part.CanCollide = false
                                end
                            end 
                        end
                    end)
                end
            else
                if ncpDflt then
                    ncpDflt:Disconnect()
                    ncpDflt = nil
                end
                for part, canCollide in pairs(ncp) do
                    if part and part.Parent then
                        part.CanCollide = canCollide
                    end
                end
                table.clear(ncp)
            end
        end
    })
end

