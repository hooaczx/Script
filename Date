local isHinhyk = false

local tomg = Instance.new("TextButton")
tomg.Size = UDim2.new(0, 260, 0, 260)
tomg.Position = UDim2.new(0.03, 10, 0.51, -125)
tomg.Text = "Game info: off"
tomg.ZIndex = 
tomg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
tomg.TextColor3 = Color3.fromRGB(255, 255, 255)
tomg.BackgroundTransparency = 1
tomg.Parent = settingsTab
tomg.MouseButton1Click:Connect(function()
    if isHinhyk then
    frame3:TweenPosition(UDim2.new(0.4, -300, 0.041, 0), "Out", "Sine", 1, true)
    tomg.Text = "Game info: on"
    else
    frame3:TweenPosition(UDim2.new(0.4, -300, 1.2, 0), "Out", "Sine", 1, true)
    tomg.Text = "Game info: off"
    end
    isHinhyk = not isHinhyk
end)

local function setUIStrokeVisibility(visible)
    local elements = {frame, toggleButton, playButton, nextButton, prevButton, speedTextBox, imageIDTextBox, shakeButton, loopButton, increaseVolumeButton, decreaseVolumeButton, settingsButton, settingsTab, bassButton, highBoostButton, lowBoostButton, echoButton, toggleImageButton, progressBar, modeToggleButton, imagetexty, toleagg, tolueagg, imageh, imageyh, frame2, frame1, sliderToggleButton}
    for _, element in ipairs(elements) do
        local stroke = element:FindFirstChildOfClass("UIStroke")
        if stroke then
            stroke.Enabled = visible
        end
    end
end

local areStrokesVisible = true

local function toggleAllOnOff()
    areStrokesVisible = not areStrokesVisible
    setUIStrokeVisibility(areStrokesVisible)
    allOnOffButton.Text = areStrokesVisible and "UI Stroke: On" or "UI Stroke: Off"
end

allOnOffButton.MouseButton1Click:Connect(toggleAllOnOff)
local function updateTime()
NameLabel.Text = "Name: "..game.Players.LocalPlayer.Name
NamedLabel.Text = "Display Name: "..game.Players.LocalPlayer.DisplayName
NaumueLabel.Text = "UserId: "..game.Players.LocalPlayer.UserId
NayumeLabel.Text = "Account Age: "..game.Players.LocalPlayer.AccountAge.." Day"
NayumeLabegl.Text = "Account Age Yeare: "..formattedAge
NayumeLabyel.Text = "Account Crated: "..formattedDate
NameyLabel.Text = "Name: "..creatorName
end
game:GetService("RunService").RenderStepped:Connect(updateTime)
