modeToggleButton.MouseButton1Click:Connect(function()
    if currentMode == "Image" then
        currentMode = "Transparency"
        imageIDTextBox.PlaceholderText = "Enter Transparency (0-1)"
        modeToggleButton.Text = "Switch to Search Song"
        imagetexty.Text = "Transparency Image"
    elseif currentMode == "Transparency" then
        currentMode = "Search"
        imageIDTextBox.PlaceholderText = "Enter Name Song"
        modeToggleButton.Text = "Switch to Image ID"
        imagetexty.Text = "Search Song"
    elseif currentMode == "Search" then
        currentMode = "Image"
        imageIDTextBox.PlaceholderText = "Enter Image ID"
        modeToggleButton.Text = "Switch to Transparency"
        imagetexty.Text = "Image Id"
    end
end)

imageIDTextBox.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local inputValue = imageIDTextBox.Text
        if currentMode == "Image" then
            if tonumber(inputValue) then
                image.Image = "rbxassetid://" .. inputValue
            else
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Error";
                    Text = "Invalid Image ID";
                    Icon = "rbxassetid://18781834542";
                    Duration = 3;
                })
            end
        elseif currentMode == "Transparency" then
            local transparencyValue = tonumber(inputValue)
            if transparencyValue and transparencyValue >= 0 and transparencyValue <= 1 then
                image.ImageTransparency = transparencyValue
            else
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Error";
                    Text = "Invalid Transparency Value (must be between 0 and 1)";
                    Icon = "rbxassetid://18781834542";
                    Duration = 3;
                })
            end
        elseif currentMode == "Search" then
        local searchTerm = imageIDTextBox.Text
    local closestSong = searchSongs(searchTerm)

    if closestSong then
for i, song in ipairs(songs) do
                    if song.id == closestSong.id then
                        currentSongIndex = i
                        break
                    end
                end

                imageIDTextBox.Text = "Playing: " .. closestSong.name
                songNameLabel.Text = closestSong.name
                lastPosition = 0
                playSong(closestSong.id)
    else
        game.StarterGui:SetCore("SendNotification", {
                Title = "Feature Not Implemented";
                Text = "Searching for songs is not implemented yet.";
                Icon = "rbxassetid://18781834542";
                Duration = 3;
            })
    end
end
    end
end)
