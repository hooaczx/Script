    local color2 = Color3.fromRGB(0, 0, 0)
    local tweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true)
    local tweenGoal = {Color = color1}
    local tween = tweenService:Create(stroke, tweenInfo, tweenGoal)
    tween:Play()
end

local function createToothpasteBlackUIStroke(stroke)
    local color1 = Color3.fromRGB(0, 255, 255)
    local color2 = Color3.fromRGB(0, 0, 0)
    local tweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true)
    local tweenGoal = {Color = color1}
    local tween = tweenService:Create(stroke, tweenInfo, tweenGoal)
    tween:Play()
end
local existingGui = playerGui:FindFirstChild("MuusicGui")
if existingGui then
    local existingSound = existingGui:FindFirstChild("MuusicSound")
    if existingSound then
        existingSound:Stop()
    end
    existingGui:Destroy()
end
local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

local CreatorId = game.CreatorId
local CreatorType = game.CreatorType
local creatorName = ""
local imageUrl = ""
local imageheadUrl = ""
if CreatorType == Enum.CreatorType.User then
    local userInfo = Players:GetNameFromUserIdAsync(CreatorId)
    creatorName = userInfo
elseif CreatorType == Enum.CreatorType.Group then
    local url = "https://groups.roblox.com/v1/groups/"..CreatorId
    local response = HttpService:GetAsync(url)
    local groupInfo = HttpService:JSONDecode(response)
    creatorName = groupInfo.owner.username
end
if CreatorType == Enum.CreatorType.User then
    local userInfo = Players:GetNameFromUserIdAsync(CreatorId)
    local userId = CreatorId
    imageUrl = "https://www.roblox.com/avatar-thumbnail/image?userId=" .. userId .. "&width=420&height=420&format=png"
elseif CreatorType == Enum.CreatorType.Group then
    local url = "https://groups.roblox.com/v1/groups/"..CreatorId
    local response = HttpService:GetAsync(url)
    local groupInfo = HttpService:JSONDecode(response)
    local groupName = groupInfo.name
    imageUrl = "https://www.roblox.com/group-icon/image?groupId=" .. CreatorId .. "&width=420&height=420&format=png"
end
if CreatorType == Enum.CreatorType.User then
    local userInfo = Players:GetNameFromUserIdAsync(CreatorId)
    local userId = Players:GetUserIdFromNameAsync(userInfo)
    imageheadUrl = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. userId .. "&width=420&height=420&format=png"
elseif CreatorType == Enum.CreatorType.Group then
    local url = "https://groups.roblox.com/v1/groups/" .. CreatorId
    local response = HttpService:GetAsync(url)
    local groupInfo = HttpService:JSONDecode(response)
    local groupOwnerId = groupInfo.owner.userId
    imageheadUrl = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. groupOwnerId .. "&width=420&height=420&format=png"
end

