local switchButton = Instance.new("TextButton")
switchButton.Size = UDim2.new(0.2, 0, 0.1, 0)
switchButton.Position = UDim2.new(0.38, 0, 0.76, 0)
switchButton.Text = "Switch To Description Game"
switchButton.ZIndex = 9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999
switchButton.TextWrapped = true
switchButton.BackgroundTransparency = 1
switchButton.TextColor3 = Color3.fromRGB(255, 255, 255)
switchButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
switchButton.ZIndex = frame3.ZIndex + 1
switchButton.Parent = screenGui

local currentFrame = frame3

switchButton.MouseButton1Click:Connect(function()
    if currentFrame == frame3 then
        frame3.Visible = false
        frame4.Visible = true
        DescriptionGame.Visible = true
        currentFrame = frame4
        switchButton.Text = "Go Back"
    else
        frame4.Visible = false
        DescriptionGame.Visible = false
        frame3.Visible = true
        currentFrame = frame3
        switchButton.Text = "Switch To Description Game"
    end
end)



