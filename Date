stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
stroke.Parent = imageIDTextBox
createRainbowUIStroke(stroke)

local isLowBoost = false
local isLooping = false
local isShaking = false
local function shakeCamera()
    local camera = game.Workspace.CurrentCamera
    local shakeConnection
    shakeConnection = game:GetService("RunService").RenderStepped:Connect(function()
        if isShaking and sound.IsPlaying then
            local amplitude = 1000 * sound.PlaybackSpeed * sound.Volume
            local frequency = 1000 * sound.PlaybackSpeed * sound.Volume
            
            local x = math.random() * amplitude - amplitude / 3
            local y = math.random() * amplitude - amplitude / 3
            camera.CFrame = camera.CFrame * CFrame.new(x / frequency, y / frequency, 0)
        else
            shakeConnection:Disconnect()
        end
    end)
end
shakeButton.MouseButton1Click:Connect(function()
    isShaking = not isShaking
    if isShaking then
        shakeButton.Text = "Shake: On"
        shakeCamera()
    else
        shakeButton.Text = "Shake: Off"
    end
end)
local sound = Instance.new("Sound")
sound.Name = "MuusicSound"
sound.Parent = screenGui
loopButton.MouseButton1Click:Connect(function()
    isLooping = not isLooping
    if isLooping then
        loopButton.Image = "rbxthumb://type=Asset&id=81752682349833&w=150&h=150"
    else
        loopButton.Image = "rbxthumb://type=Asset&id=89124530711215&w=150&h=150"
    end
end)
local isBassOn = false
local function toggleBass()
    if isBassOn then
        bassButton.Text = "Bass Boost: Off"
        sound.Volume = math.max(sound.Volume - 0.2, 0.1)
    else
        bassButton.Text = "Bass Boost: On"
        sound.Volume = math.min(sound.Volume + 0.2, 2.5)
    end
    isBassOn = not isBassOn
end
bassButton.MouseButton1Click:Connect(toggleBass)
local function toggleHighBoost()
    isHighBoost = not isHighBoost
    if isHighBoost then
        highBoostButton.Text = "High Boost: On"
        sound.Volume = math.min(sound.Volume * 2, 2.5)
    else
        highBoostButton.Text = "High Boost: Off"
        sound.Volume = math.max(sound.Volume / 2, 0)
    end
end
highBoostButton.MouseButton1Click:Connect(toggleHighBoost)
local function toggleLowBoost()
    isLowBoost = not isLowBoost
    if isLowBoost then
        lowBoostButton.Text = "Low Boost: On"
        sound.PlaybackSpeed = 0.5
    else
        lowBoostButton.Text = "Low Boost: Off"
        sound.PlaybackSpeed = tonumber(speedTextBox.Text) or 1
    end
end
lowBoostButton.MouseButton1Click:Connect(toggleLowBoost)
