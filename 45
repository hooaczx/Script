    local pages = AssetService:GetGamePlacesAsync()
    local gamePlaces = pages:GetCurrentPage()
    local count = 1
    local image = Configs.Image or ""

    local frame = Instance.new("Frame", ScreenGui)
    frame.Size = UDim2.new(0.5, 0, 0.12, 0)
    frame.Position = UDim2.new(0.23, 0, 0.878, 0)
    frame.BackgroundColor3 = Configs_HUB.Cor_Hub

    local tweenGradient = CreateGradient(frame, {
    Color = {
        Color3.fromRGB(255, 255, 255),
        Color3.fromRGB(0, 0, 0),
        Color3.fromRGB(255, 0, 0)
    },
    Rotation = 45,
    Tween = true,
    Speed = 3
    })
    if stroke then Stroke(frame) end

    local Button1 = Instance.new("TextButton", ScreenGui)
    Button1.Size = UDim2.new(0, 30, 0, 30)
    Button1.Position = UDim2.new(0.4713, 0, 0.803, 0)
    Button1.TextScaled = true
    Button1.Text = "v"
    Button1.ZIndex = 10
    Button1.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button1.BackgroundTransparency = 1

 local minimize1 = true
    Button1.MouseButton1Click:Connect(function()
        minimize1 = not minimize1

        if minimize1 then
            CreateTween1(frame, "Size", UDim2.new(0.5, 0, 0.001, 0), 0.3, true)
            CreateTween1(Button1, "Position", UDim2.new(0.4713, 0, 0.943, 0), 0.3, true)
            frame.Visible = false
            Button1.Text = "^"
        else
            frame.Visible = true
            CreateTween1(frame, "Size", UDim2.new(0.5, 0, 0.12, 0), 0.3, true)
            CreateTween1(Button1, "Position", UDim2.new(0.4713, 0, 0.803, 0), 0.3, true) 
            Button1.Text = "v"
        end
    end)


