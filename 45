local notifications = {}

local function notify(message)
    local NotificationLabel = Instance.new("TextLabel", ScreenGui) 
    NotificationLabel.Size = UDim2.new(0, 400, 0, 70)
    NotificationLabel.BackgroundTransparency = 1
    NotificationLabel.Text = message
    NotificationLabel.TextScaled = true
    NotificationLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotificationLabel.Position = UDim2.new(0.5, -200, 0.45, 50)
    
    local NotificationLabelText = Instance.new("TextLabel", NotificationLabel)
    NotificationLabelText.Size = UDim2.new(1, 0, 0.4, 0)
    NotificationLabelText.BackgroundTransparency = 1
    NotificationLabelText.Text = "Attempted to copy"
    NotificationLabelText.TextScaled = true
    NotificationLabelText.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotificationLabelText.Position = UDim2.new(0, 0, -0.4, 0)
    
    local OkButton = Instance.new("TextButton", NotificationLabel) 
    OkButton.Size = UDim2.new(1, 0, 0.4, 0)
    OkButton.Position = UDim2.new(0, 0, 1, 0)
    OkButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    OkButton.TextScaled = true
    OkButton.Text = "OK"
    OkButton.BackgroundTransparency = 1

    table.insert(notifications, NotificationLabel)

    if #notifications > 1 then
        local oldNotification = table.remove(notifications, 1)
        oldNotification:Destroy()
    end
    
    local tween = TweenService:Create(NotificationLabel, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -200, 0.45, 50 - (#notifications * 60))})
    tween:Play()
    
    OkButton.MouseButton1Click:Connect(function()
        local tweenOut = TweenService:Create(NotificationLabel, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -200, 0.45, 100), TextTransparency = 1})
        tweenOut:Play()

        tweenOut.Completed:Connect(function()
            NotificationLabel:Destroy()
        end)
    end)
end

local copyEnabled = false

local originalToClipboard = getgenv().toclipboard
local originalSetClipboard = getgenv().setclipboard
local originalOriginalSetClipboard = getgenv().originalSetClipboard

getgenv().toggleClipboard = function(copyEnabled)
    if copyEnabled then
    getgenv().toclipboard = function(data)
            notify("" .. tostring(data))
        end
        getgenv().setclipboard = function(data)
            notify("" .. tostring(data))
        end
        getgenv().originalSetClipboard = function(data)
            notify("" .. tostring(data))
        end
    else
        getgenv().toclipboard = originalToClipboard
        getgenv().setclipboard = originalSetClipboard
        getgenv().originalSetClipboard = originalOriginalSetClipboard
    end
end

local TogglecopyButton = createToggleButton(frameAnticopy, UDim2.new(0.83, 0, 0.75, 0), function(state)
    toggleClipboard(state)
end, copyEnabled)

