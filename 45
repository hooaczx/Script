local function notifyPlayer(player, action)
    if not notifyFriendsEnabled then return end

    local NotificationFrame = Instance.new("Frame", ScreenGui)
    NotificationFrame.Size = UDim2.new(0.315, 0, 0.1, 0)
    NotificationFrame.BackgroundTransparency = 1
    NotificationFrame.Position = UDim2.new(0.56, -200, 0.6, 50)

    local ProfileImage = Instance.new("ImageLabel", NotificationFrame)
    ProfileImage.Size = UDim2.new(0, 45, 1, 0)
    ProfileImage.BackgroundTransparency = 1
    ProfileImage.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. player.UserId .. "&width=420&height=420&format=png"

    local NotificationLabel = Instance.new("TextLabel", NotificationFrame)
    NotificationLabel.Size = UDim2.new(0.85, 0, 1, 0)
    NotificationLabel.BackgroundTransparency = 1
    NotificationLabel.Position = UDim2.new(0, 45, 0, 0)
    NotificationLabel.Text = player.Name .. " " .. action
    NotificationLabel.TextScaled = true
    NotificationLabel.TextXAlignment = Enum.TextXAlignment.Left
    NotificationLabel.TextColor3 = Color3.fromRGB(255, 255, 255)

    local ProgressBar = Instance.new("Frame")
    ProgressBar.Parent = NotificationFrame
    ProgressBar.Size = UDim2.new(1.016, 0, 0, 5)
    ProgressBar.BackgroundColor3 = Color3.fromRGB(0, 255, 0)

    table.insert(friendnotifications, NotificationFrame)

    if #friendnotifications > 5 then
        local oldNotification = table.remove(friendnotifications, 1)
        oldNotification:Destroy()
    end

    for i, notifyFrame in ipairs(friendnotifications) do
        local tween = TweenService:Create(notifyFrame, TweenInfo.new(0.5), {Position = UDim2.new(0.56, -200, 0.6, 50 - (i * 80))})
        tween:Play()
    end

    local timeLeft = 10
    local startTime = tick()

    local updateConnection
    updateConnection = RunService.Heartbeat:Connect(function()
        local elapsedTime = tick() - startTime
        local remainingTime = timeLeft - elapsedTime

        if remainingTime <= 0 then
            remainingTime = 0
            updateConnection:Disconnect()
        end

        ProgressBar.Size = UDim2.new(remainingTime / timeLeft, 0, 0, 5)

        if remainingTime <= 0 then
            local tweenOut = TweenService:Create(NotificationFrame, TweenInfo.new(0.5), {Position = UDim2.new(0.56, -200, 0.6, 100), BackgroundTransparency = 1})
            tweenOut:Play()

            tweenOut.Completed:Connect(function()
                NotificationFrame:Destroy()
                table.remove(friendnotifications, table.find(friendnotifications, NotificationFrame))

                for i, notifyFrame in ipairs(friendnotifications) do
                    local tween = TweenService:Create(notifyFrame, TweenInfo.new(0.5), {Position = UDim2.new(0.56, -200, 0.6, 50 - (i * 80))})
                    tween:Play()
                end
            end)
        end
    end)
end

game.Players.ChildAdded:Connect(function(player)
    local isFriend = false
    local success, friendsPage = pcall(Players.GetFriendsAsync, Players, localPlayer.UserId)

    if success then
        repeat
            local friendsList = friendsPage:GetCurrentPage()
            for _, friendInfo in pairs(friendsList) do
                if friendInfo.Username == player.Name then
                    isFriend = true
                    break
                end
            end
            if not friendsPage.IsFinished then
                friendsPage:AdvanceToNextPageAsync()
            end
        until friendsPage.IsFinished or isFriend
    end

    if isFriend then
        notifyPlayer(player, "Friend has joined the game")
    end
end)

game.Players.ChildRemoved:Connect(function(player)
    local isFriend = false
    local success, friendsPage = pcall(Players.GetFriendsAsync, Players, localPlayer.UserId)

    if success then
        repeat
            local friendsList = friendsPage:GetCurrentPage()
            for _, friendInfo in pairs(friendsList) do
                if friendInfo.Username == player.Name then
                    isFriend = true
                    break
                end
            end
            if not friendsPage.IsFinished then
                friendsPage:AdvanceToNextPageAsync()
            end
        until friendsPage.IsFinished or isFriend
    end

    if isFriend then
        notifyPlayer(player, "Friend has left the game")
    end
end)

    local SettingButton = Instance.new("ImageButton", frame)
    SettingButton.Size = UDim2.new(0, 30, 0, 30)
    SettingButton.ZIndex = 10
    SettingButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    SettingButton.BackgroundTransparency = 1
    SettingButton.Position = UDim2.new(0.85, 0, 0, 13)
    SettingButton.Image = image

    if corner then Corner(SettingButton) end 
    if stroke then Stroke(SettingButton, {Color = strokecolor}) end
    
    local minimize9 = false
    SettingButton.MouseButton1Click:Connect(function()
     minimize9 = not minimize9

        if minimize9 then
            frameSetting.Visible = true
            CreateTween(frameSetting, "Size", UDim2.new(0.4, 0, 0.8, 0), 0.3, false)
        else
            CreateTween(frameSetting, "Size", UDim2.new(0.4, 0, 0, 0), 0.3, true)
            frameSetting.Visible = false
        end
    end)
end
  
