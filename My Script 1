local Lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Hosvile/Refinement/main/InfinitiveUI",true))()

local Win = Lib:CreateWindow("1hooaczx1")

local Tab,name = Win:CreateTab("Script "..tostring(i),function() warn(i) end)

Tab:CreateButton("1hooaczx1",function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/hooaczx/Script/main/1hooaczx1"))()
end) 

Tab:CreateButton("My Script",function()
loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/hooaczx/Script/main/My%20Script"))()
end)
