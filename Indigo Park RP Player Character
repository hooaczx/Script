local Tab2 = Window:MakeTab({"Face", "user"})

Tab2:AddButton({"Face|Rambley|Introduce",function()
game:GetService("ReplicatedStorage").Voicelines.Rambley.Happy:FireServer()
end})

Tab2:AddButton({"Stop|Face|Rambley|Spin",function()
game:GetService("ReplicatedStorage").Voicelines.Rambley.StopHappy:FireServer()
end})

Tab2:AddButton({"Face|Rambley|Introduce",function()
