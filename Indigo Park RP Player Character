local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/RedzLibV5/main/Source.Lua"))()

local Window = redzlib:MakeWindow({
  Title = "Indigo Park RP Player Character",
  SubTitle = "Welcome to Script hooaczx & 1hooaczx1 • TH",
  SaveFolder = "Redz Config"
})

local Tab1 = Window:MakeTab({"Player Character", "user"})

Tab1:AddButton({"Go Return To Old Gui",function()
loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/hooaczx/Script/main/Indigo%20Park%20RP"))()
end})

Tab1:AddButton({"Neme Tag hooaczx & 1hooacx1",function()
local ohString1 = "hooaczx & 1hooacx1"  

game:GetService("ReplicatedStorage").ChangeNameTag:FireServer(ohString1)
end})

Tab1:AddButton({"Neme Tag hooaczx & 1hooacx1 (แบบทำซ้ำไปเรื่อยๆ)🔖",function()
_G.H = true 
while _G.H do wait()
wait(.0)
local ohString1 = "hooaczx & 1hooacx1"  

game:GetService("ReplicatedStorage").ChangeNameTag:FireServer(ohString1)
end
end})

