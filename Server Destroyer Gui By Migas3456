local function NJUNBHQ_fake_script() -- A.LocalScript 
	local script = Instance.new('LocalScript', A)

	script.Parent.MouseButton1Down:Connect(function()
		local aframe = script.Parent.Parent.Parent.AnthemsFrame
		if aframe.Visible == false then
			aframe.Visible = true
		else
			aframe.Visible = false
		end
	end)
	
end
