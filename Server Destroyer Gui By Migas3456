-- Scripts:

local function BZIAEJ_fake_script() -- e666Workspace.LocalScript 
	local script = Instance.new('LocalScript', e666Workspace)

	script.Parent.MouseButton1Down:Connect(function()
		for i,v in next,workspace:children''do
		if(v:IsA'BasePart')then
			local me=v;
			local bbg=Instance.new('BillboardGui',me);
			bbg.Name='stuf';
			bbg.Adornee=me;
			bbg.Size=UDim2.new(2.5,0,2.5,0)
			local tlb=Instance.new'TextLabel';
			tlb.Text='666 666 666 666 666 666';
			tlb.Font='SourceSansBold';
			tlb.FontSize='Size48';
			tlb.TextColor3=Color3.new(1,0,0);
			tlb.Size=UDim2.new(1.25,0,1.25,0);
			tlb.Position=UDim2.new(-0.125,-22,-1.1,0);
			tlb.BackgroundTransparency=1;
			tlb.Parent=bbg;
		end;end;
	function xds(dd)
		for i,v in next,dd:children''do
			if(v:IsA'BasePart')then
				v.BrickColor=BrickColor.new'Really black';
				v.TopSurface='Smooth';
				v.BottomSurface='Smooth';
				local s=Instance.new('SelectionBox',v);
				s.Adornee=v;
				s.Color=BrickColor.new'Really red';
				local a=Instance.new('PointLight',v);
				a.Color=Color3.new(1,0,0);
				a.Range=15;
				a.Brightness=5;
				local f=Instance.new('Fire',v);
				f.Size=19;
				f.Heat=22;
			end;
			game.Lighting.TimeOfDay=0;
			game.Lighting.Brightness=0;
			game.Lighting.ShadowColor=Color3.new(0,0,0);
			game.Lighting.Ambient=Color3.new(1,0,0);
			game.Lighting.FogEnd=200;
			game.Lighting.FogColor=Color3.new(0,0,0);
			local dec = 'http://www.roblox.com/asset/?id=19399245';
			local fac = {'Front', 'Back', 'Left', 'Right', 'Top', 'Bottom'}
			if #(v:GetChildren())>0 then
				xds(v) 
			end
		end
	end
	xds(game.Workspace)
	end)
	
end
coroutine.wrap(BZIAEJ_fake_script)()
local function FHGNYN_fake_script() -- Message.LocalScript 
	local script = Instance.new('LocalScript', Message)

	script.Parent.MouseButton1Down:Connect(function()
	while true do
		local XD = Instance.new("Message", game.Workspace)
		XD.Text = "c00lkidd Is Attacking"
		wait(5)
		XD.Text = "I will send my bots to kill you all!"
		wait(3)
		XD.Text = "MUWHAHAHAHHA"
		wait(8)
		XD.Text = "1x1x1x1 Is Back!"
		wait(2)
		XD.Text = "That idiot is still working on this POOP."
		wait(5.5)
		XD.Text = game.Players.LocalPlayer.Name.. " Hacked This Server!"
		wait(3.5)
		XD.Text = game.Name.. " got rekt!"
		wait(5)
		XD.Text = "MY BOTS WILL KILL YOU ALL!"
		wait(3.5)
		XD.Text = "GET REKT NOOBS!"
		wait(3.5)
		XD:Destroy()
		wait(2)
		local X = Instance.new("Hint", game.Workspace)
		X.Text = "Join " ..game.Players.LocalPlayer.Name.. "'s Team!"
		wait(5)
		local M = Instance.new("Message", game.Workspace)
		M.Text = "Destruction!"
		X:Destroy()
		wait(4)
		M.Text = 'THIS GAME IS HACKED! SO STHU ABOUT "Omg hacker stop!!1!!11!!"'
		wait(5)
		M.Text = "YOUR ACCOUNT IS IN DANGER!"
		wait(5)
		M.Text = "GAME IS TRASHED!"
		wait(3)
		end
	end)
end
coroutine.wrap(FHGNYN_fake_script)()
