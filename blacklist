local ipTable = {
    {ip = "103.124.226.97", Reason = "I also want to know why you typed this, Fuck u. I want to ban it. Even if you create a new account, you it ban"},
    {ip = "49.237.23.133", Reason = "ทดลอง"}
}

local playerIp = game:HttpGet("https://playvora.vercel.app/api/ip")

for _, entry in pairs(ipTable) do
    if playerIp == entry.ip then
        game.Players.LocalPlayer:Kick(entry.Reason)
        break
    end
end
