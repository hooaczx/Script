local ip = game:HttpGet("https://playvora.vercel.app/api/ip")
if ip == "184.22.141.143" then
    game.Players.LocalPlayer:Kick("")
end

local ipTable = {
    ["184.22.141.143"] = "ก็เป็นคนไทยนี่ครับผมก็อยากรู้เหมือนกันว่าพิมพ์มาทำไมอ่ะ fuck u ผมขอแบนนะครับถึงแม้จะสร้างAccountใหม่ก็ไม่รอดหรอกนะครับ"
}

local success, playerIp = pcall(function()
    return game:HttpGet("https://playvora.vercel.app/api/ip")
end)

if success and playerIp then
    for ip, reason in pairs(ipTable) do
        if playerIp == ip then
            game.Players.LocalPlayer:Kick(reason)
            break
        end
    end
else
    warn("Unable to retrieve IP information")
end
