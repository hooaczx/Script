local ipTable = {
    ["103.124.226.97"] = "I also want to know why you typed this, Fuck u. I want to ban it. Even if you create a new account, you it ban",
    ["49.237.23.133"] = "ทดลอง"
}

if success and playerIp then
    for ip, reason in pairs(ipTable) do
        if playerIp == ip then
            game.Players.LocalPlayer:Kick(reason)
            break
        end
    end
else
    warn("Unable to retrieve IP information")
end
