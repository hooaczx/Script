endlocal function playSong(songId)
    sound.SoundId = songId
    sound:Play()
    lastPosition = 0
end
local function searchSongs(query)
    local closestMatch = nil
    local closestDistance = math.huge
    for _, song in ipairs(songs) do
        local distance = string.len(song.name) - string.len(query) -- Simple distance calculation
        if string.find(string.lower(song.name), string.lower(query)) then
            if distance < closestDistance then
                closestDistance = distance
                closestMatch = song
            end
        end
    end
    return closestMatch
endlocal function playNextSong()
    if not isLooping then
        currentSongIndex = currentSongIndex + 1
        if currentSongIndex > #songs then
            currentSongIndex = 1
        end
    end
    lastPosition = 0
    playSong()
endlocal function pauseSong()
    lastPosition = sound.TimePosition
    sound:Pause()
    playButton.Image = "rbxthumb://type=Asset&id=120936263094449&w=150&h=150"
endlocal function playSong()
    sound.SoundId = songs[currentSongIndex].id
    sound.TimePosition = lastPosition
    sound:Play()
    playButton.Image = "rbxthumb://type=Asset&id=133872094700280&w=150&h=150"
    songNameLabel.Text = songs[currentSongIndex].name
endlocal function updateRealTime()
local currentTime = os.date("*t")
    local hour = currentTime.hour
    local amPm = "AM"
    if hour >= 12 then
        amPm = "PM"
        if hour > 12 then
            hour = hour - 12
        end
    elseif hour == 0 then
        hour = 12
    end
    local formattedTime = string.format("%02d:%02d:%02d %s", hour, currentTime.min, currentTime.sec, amPm)
    local formatted_date = string.format("%02d/%02d/%04d", currentTime.day, currentTime.month, currentTime.year)
    timeLabelReal.Text = "Time:  " .. formattedTime.." : "..formatted_date
endlocal function createPastelyellowUIStroke(stroke)
    local color1 = Color3.fromRGB(255, 255, 204)
