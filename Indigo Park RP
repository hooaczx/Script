local players = game:GetService("Players")
game.StarterGui:SetCore("SendNotification", {
    Title = "Hi";
    Text = "" .. players.LocalPlayer.DisplayName;
    Icon = "rbxassetid://17619581667";
    Icon = "rbxthumb://type=Asset&id=17619581667&w=150&h=150";
    Duration = 9999999999999999999999999999;
    callbakc = bindableFunction;
    Button1 = "Hi";
})
local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/hooaczx/Script/main/RedzHub%20Ui%20library"))()

local Window = redzlib:MakeWindow({
  Title = "Indigo Park RP",
  SubTitle = "Welcome to Script hooaczx & 1hooaczx1 â€¢ TH",
  SaveFolder = "Redz Config"
})

local Tab1 = Window:MakeTab({"Morph", "user"})

Tab1:AddButton({"Player Character",function()
loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/hooaczx/Script/main/Indigo%20Park%20RP%20Player%20Character"))()
end})

Tab1:AddButton({"LLOYD",function()
local ohInstance1 = game:GetService("ReplicatedStorage").Monsters.Catnap

game:GetService("ReplicatedStorage").CharChange:FireServer(ohInstance1)
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/hooaczx/Script/main/212's%20Notification%20Library"))()

library:CreateNotification(3, "CenterThenBottom", "Hi", "Have fun using this character", "rbxassetid://17599570595")
end})

