local currentSongIndex = 1
local soundmusic = Instance.new("Sound")
soundmusic.Parent = ScreenGui
local isPaused = false
local savedTimePosition = 0
local progressUpdateConnection
local soundId1 = "rbxassetid://478544929"
local soundId2 = "rbxassetid://4499400560"
local clicktoggle = Instance.new("Sound", ScreenGui)
clicktoggle.SoundId = "rbxassetid://6042053626"
clicktoggle.Volume = 0.1
local falsetoggle = Instance.new("Sound", ScreenGui)
falsetoggle.SoundId = "rbxassetid://9083627113"
falsetoggle.Volume = 0.1
local click1 = Instance.new("Sound", ScreenGui)
click1.SoundId = "rbxassetid://7475853483"
click1.Volume = 0.1
local click2 = Instance.new("Sound", ScreenGui)
click2.SoundId = "rbxassetid://8432836186"
click2.Volume = 0.1
local notifyfriend = Instance.new("Sound", ScreenGui)
notifyfriend.SoundId = "rbxassetid://1862045322"
notifyfriend.Volume = 0.2
notifyfriend.PlaybackSpeed = 0.9
local notifyfriendleave = Instance.new("Sound", ScreenGui)
notifyfriendleave.SoundId = "rbxassetid://489103549"
notifyfriendleave.Volume = 0.2

local function resetSong()
    soundmusic:Stop()
    savedTimePosition = 0
    isPaused = false
end

local function pauseSong()
    savedTimePosition = soundmusic.TimePosition
    soundmusic:Pause()
    isPaused = true
end

function PlaySongByIndex(index, TitleLabel, TimeLabel, Frame_4)
    local song = songs[index]
    if song then
        soundmusic.SoundId = song.id
        soundmusic.TimePosition = savedTimePosition
        soundmusic:Play()
        TitleLabel.Text = song.name
        StartProgressUpdate(Frame_4, TimeLabel)
    end
end

function FormatTime(seconds)
    local minutes = math.floor(seconds / 60)
    local remainingSeconds = math.floor(seconds % 60)
    return string.format("%02d:%02d", minutes, remainingSeconds)
end

function StartProgressUpdate(Frame_4, TimeLabel)
    if progressUpdateConnection then
        progressUpdateConnection:Disconnect()
    end

    progressUpdateConnection = RunService.Heartbeat:Connect(function()
        if soundmusic.IsPlaying then
            function StartProgressUpdate(Frame_4, TimeLabel)
    if progressUpdateConnection then
        progressUpdateConnection:Disconnect()
    end

    progressUpdateConnection = RunService.Heartbeat:Connect(function() 
        if soundmusic.IsPlaying then
            Frame_4.Rotation = (Frame_4.Rotation + soundmusic.PlaybackSpeed * soundmusic.Volume + 0.3) % 360
            TimeLabel.Text = FormatTime(soundmusic.TimePosition) .. " / " .. FormatTime(soundmusic.TimeLength)
        else
            if progressUpdateConnection then
                progressUpdateConnection:Disconnect()
                progressUpdateConnection = nil
            end
        end
    end)
end
            TimeLabel.Text = FormatTime(soundmusic.TimePosition) .. " / " .. FormatTime(soundmusic.TimeLength)
        else
            if progressUpdateConnection then
                progressUpdateConnection:Disconnect()
                progressUpdateConnection = nil
            end
        end
    end)
end

local notificationgui = {}

local function notify(params)
    local Title = params.Title or "Default Title"
    local Text = params.Text or "Default Message"
    local buttondeleteText = params.buttonText or "OK"
    local image = params.image or "18488884411"
    local guiSize = params.guiSize or Vector2.new(400, 70)
    local duration = params.duration
    local position = params.Position or UDim2.new(0.5, -guiSize.X / 2, 0.45, 50)

    local NotificationLabelText = Instance.new("TextLabel", ScreenGui) 
    NotificationLabelText.Size = UDim2.new(0, guiSize.X, 0, guiSize.Y)
    NotificationLabelText.BackgroundTransparency = 1
    NotificationLabelText.Text = Text
    NotificationLabelText.TextScaled = true
    NotificationLabelText.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotificationLabelText.Position = position

    local sound2 = Instance.new("Sound", NotificationLabelText)
    sound2.SoundId = soundId1
    sound2.Volume = 0.2
    sound2.PlaybackSpeed = 0.9
    sound2:Play()

    local NotificationImage = Instance.new("ImageLabel", NotificationLabelText)
    NotificationImage.Size = UDim2.new(0, 70, 0, 70)
    NotificationImage.Position = UDim2.new(-0.2, 0, 0, 0)
    NotificationImage.Image = "rbxassetid://" .. image
    NotificationImage.BackgroundTransparency = 1
    
    local NotificationLabelTitle = Instance.new("TextLabel", NotificationLabelText)
    NotificationLabelTitle.Size = UDim2.new(1, 0, 0.4, 0)
    NotificationLabelTitle.BackgroundTransparency = 1
    NotificationLabelTitle.Text = Title
    NotificationLabelTitle.TextScaled = true
    NotificationLabelTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotificationLabelTitle.Position = UDim2.new(0, 0, -0.4, 0)

    local OkButton = Instance.new("TextButton", NotificationLabelText) 
    OkButton.Size = UDim2.new(1, 0, 0.4, 0)
    OkButton.Position = UDim2.new(0, 0, 1, 0)
    OkButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    OkButton.TextScaled = true
    OkButton.Text = buttondeleteText
    OkButton.BackgroundTransparency = 1

    local sound1 = Instance.new("Sound", OkButton)
    sound1.SoundId = soundId2
    sound1.Volume = 0.2
    sound1.PlaybackSpeed = 0.9

    table.insert(notificationgui, NotificationLabelText)

    if #notificationgui > 1 then
        local oldNotification = table.remove(notificationgui, 1)
        oldNotification:Destroy()
    end
    
    local tween = TweenService:Create(NotificationLabelText, TweenInfo.new(0.5), {Position = UDim2.new(position.X.Scale, position.X.Offset, position.Y.Scale, position.Y.Offset - (#notificationgui * 60))})
    tween:Play()
    
    if duration then
        delay(duration, function()
            local tweenOut = TweenService:Create(NotificationLabelText, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -guiSize.X / 2, 0.45, 100), TextTransparency = 1})
            tweenOut:Play()
            
            tweenOut.Completed:Connect(function()
                NotificationLabelText:Destroy()
            end)
        end)
    end

    OkButton.MouseButton1Click:Connect(function()
        sound1:Play()
        local tweenOut = TweenService:Create(NotificationLabelText, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -guiSize.X / 2, 0.45, 100), TextTransparency = 1})
        tweenOut:Play()
        
        tweenOut.Completed:Connect(function()
            sound2:Destroy()
            sound1:Destroy()
            NotificationLabelText:Destroy()
        end)
    end)
end

notify({
      Title = "Hi",
      Text = "Hope you enjoy using it",
      buttonText = "ok",
      imageID = "",
      guiSize = Vector2.new(400, 70),
      })

  function MinimizeButton(Configs)
    local player = game.Players.LocalPlayer
    local players = game:GetService("Players")
    local localPlayer = players.LocalPlayer
    local LocalPlayer = players.LocalPlayer
    local accountAgeInDays = LocalPlayer.AccountAge
    local maxPlayers = players.MaxPlayers
    local ageInDays = player.AccountAge
    local PlaceId = game.PlaceId
    local friendsCooldown = 0
    local friendsInTotal = 0
    local MarketplaceService = game:GetService("MarketplaceService")
    local AssetService = game:GetService("AssetService")
    local TeleportService = game:GetService("TeleportService")
    local pages = AssetService:GetGamePlacesAsync()
    local gamePlaces = pages:GetCurrentPage()
    local count = 1
    local Player = players.LocalPlayer
    local ContextActionService = game:GetService("ContextActionService")
    local UserInputService = game:GetService("UserInputService")
    local image = Configs.Image or ""

    local frame = Instance.new("Frame", ScreenGui)
    frame.Size = UDim2.new(0, 430, 0, 50)
    frame.AnchorPoint = Vector2.new(0.5, 1)
    frame.Position = UDim2.new(0.48, 0, 1, 0)
    frame.BackgroundColor3 = Configs_HUB.Cor_Hub

    local tweenGradient = CreateGradient(frame, {
    Color = {
        Color3.fromRGB(255, 255, 255),
        Color3.fromRGB(0, 0, 0),
        Color3.fromRGB(255, 0, 0)
    },
    Rotation = 45,
    Tween = true,
    Speed = 3
    })

    local Button1 = Instance.new("TextButton", ScreenGui)
    Button1.Size = UDim2.new(0, 30, 0, 30)
    Button1.AnchorPoint = Vector2.new(0.5, 1)
    Button1.Position = UDim2.new(0.48, 0, 1, -50)
    Button1.TextScaled = true
    Button1.Text = "v"
    Button1.ZIndex = 10
    Button1.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button1.BackgroundTransparency = 1

    local minimize1 = false

Button1.MouseButton1Click:Connect(function()
    minimize1 = not minimize1

    local settings = minimize1 and {
        UDim2.new(0, 430, 0, 0), 
        UDim2.new(0.48, 0, 1, 0), 
        false, 
        "^"
    } or {
        UDim2.new(0, 430, 0, 50), 
        UDim2.new(0.48, 0, 1, -60), 
        true, 
        "v"
    }

    CreateTween1(frame, "Size", settings[1], 0.3, false)
    CreateTween1(Button1, "Position", settings[2], 0.3, false)
    frame.Visible = settings[3]
    Button1.Text = settings[4]
end)

