[[Make the song repeat If you don't want it, you can turn it off]],
function(isLooping)
isAutoPlayEnabled = false
isLooping = not isLooping
end, isLooping)

local AutoPlayframe = functions.createToggleButton(AutoPlayframe, UDim2.new(0.82, 0, 0.75, 0), function(isAutoPlayEnabled)
isLooping = false
isAutoPlayEnabled = not isAutoPlayEnabled
end, isAutoPlayEnabled)

local function onSongEnded()
    if isLooping then
        PlaySongByIndex(currentSongIndex, TitleLabel, TimeLabel, Frame_4)
    elseif isAutoPlayEnabled then
        currentSongIndex = currentSongIndex + 1
        if currentSongIndex > #songs then
            currentSongIndex = 1
        end
        resetSong()
        PlaySongByIndex(currentSongIndex, TitleLabel, TimeLabel, Frame_4)
        updateSongButtons()
    end
end

soundmusic.Ended:Connect(onSongEnded)

