local songList = Instance.new("ScrollingFrame", Frame1)
songList.BackgroundColor3 = Color3.new(0, 0, 0)
songList.BorderColor3 = Color3.new(0.223529, 0.223529, 0.223529)
songList.Size = UDim2.new(1, 0, 0.8, 0)
songList.Position = UDim2.new(0, 0, 0.21, 0)
songList.ScrollBarThickness = 0
songList.BackgroundTransparency = 1
songList.ScrollingEnabled = true

local songListTinniLayout = Instance.new("UIListLayout", songList) 
songListTinniLayout.SortOrder = Enum.SortOrder.LayoutOrder

local function updateFrameSize()
    local contentHeight = songListTinniLayout.AbsoluteContentSize.Y
    songList.CanvasSize = UDim2.new(0, 0, 0, contentHeight)
end

local isAutoPlayEnabled = false
local isLooping = false

local function updateSongButtons()
    local searchText = search.Text:lower()
    local currentScrollPosition = songList.CanvasPosition
    for _, child in ipairs(songList:GetChildren()) do
        if child:IsA("Frame") and child.Name ~= "UIListLayout" then
            child:Destroy()
        end
    end
    for i, song in ipairs(songs) do
        if string.lower(song.name):find(searchText, 1, true) then
            local musiclistFrame = Instance.new("Frame", songList)
            musiclistFrame.BackgroundColor3 = Color3.new(0, 0, 0)
            musiclistFrame.Size = UDim2.new(1, 0, 0, 25)
            musiclistFrame.BorderSizePixel = 0
            musiclistFrame.BackgroundTransparency = 0

            if not musiclistFrame:FindFirstChild("NameLabel") then
                local iconmusic = Instance.new("ImageLabel", musiclistFrame)
                iconmusic.Size = UDim2.new(0.1, 0, 1, 0)
                iconmusic.Position = UDim2.new(0, 0, 0, 0)
                iconmusic.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                iconmusic.BackgroundTransparency = 1
                iconmusic.Image = "rbxthumb://type=Asset&id=107723524907022&w=420&h=420"

                local nameLabel = Instance.new("TextLabel", musiclistFrame)
                nameLabel.Name = "NameLabel"
                nameLabel.Text = ":" .. tostring(i) .. "." .. song.name
                nameLabel.Size = UDim2.new(0.61, 0, 1, 0)
                nameLabel.Position = UDim2.new(0.11, 0, 0, 0)
                nameLabel.TextScaled = true
                nameLabel.TextColor3 = Configs_HUB.Cor_Text
                nameLabel.TextXAlignment = "Left"
                nameLabel.Font = Configs_HUB.Text_Font
                nameLabel.BackgroundTransparency = 1
                nameLabel.BorderSizePixel = 0
                TextSetColor(nameLabel)
            end

            if not musiclistFrame:FindFirstChild("SongButton") then
                local songbutton = Instance.new("TextButton", musiclistFrame)
                songbutton.Name = "SongButton"
                songbutton.Size = UDim2.new(1, 0, 1, 0)
                songbutton.Position = UDim2.new(0, 0, 0, 0)
                songbutton.TextColor3 = Configs_HUB.Cor_Text
                songbutton.Font = Configs_HUB.Text_Font
                songbutton.Text = ""
                songbutton.BackgroundTransparency = 1
                TextSetColor(songbutton)

                songbutton.MouseButton1Click:Connect(function()
                if isLooping then
                        currentSongIndex = i
                        PlaySongByIndex(currentSongIndex, TitleLabel, TimeLabel, Frame_4)
                        ToggleButton.Image = "rbxthumb://type=Asset&id=133872094700280&w=150&h=150"
                        updateSongButtons() 
                   end
                end)
            end

            if currentSongIndex == i then
                local frameimage = Instance.new("Frame", musiclistFrame)
                frameimage.Size = UDim2.new(0, 30, 1, 0)
                frameimage.Position = UDim2.new(0.85, 0, 0, 0)
                frameimage.BackgroundTransparency = 1

                for _, child in ipairs(frameimage:GetChildren()) do
                    if child:IsA("ImageLabel") then
                        child:Destroy()
                    end
                end

                local image = Instance.new("ImageLabel", frameimage)
                image.Size = UDim2.new(1, 0, 1, 0)
                image.BackgroundTransparency = 1
                image.Image = "rbxthumb://type=Asset&id=18488884411&w=150&h=150"
            end
        end
    end
    songList.CanvasPosition = currentScrollPosition
    updateFrameSize()
end

updateSongButtons()

search:GetPropertyChangedSignal("Text"):Connect(updateSongButtons)

NextButton.MouseButton1Click:Connect(function()
    if not isLooping then
        resetSong()
        currentSongIndex = (currentSongIndex % #songs) + 1
        PlaySongByIndex(currentSongIndex, TitleLabel, TimeLabel, Frame_4)
        updateSongButtons()
        ToggleButton.Image = "rbxthumb://type=Asset&id=133872094700280&w=150&h=150"
    end
end)

PreviousButton.MouseButton1Click:Connect(function()
    if not isLooping then
        resetSong()
        currentSongIndex = (currentSongIndex - 2 + #songs) % #songs + 1
        PlaySongByIndex(currentSongIndex, TitleLabel, TimeLabel, Frame_4)
        updateSongButtons()
        ToggleButton.Image = "rbxthumb://type=Asset&id=133872094700280&w=150&h=150"
    end
end)

local functions = {}

functions.createToggleButton = function(parent, position, callback, initialState)
    local TextButton = Instance.new("TextButton", parent)
    TextButton.Size = UDim2.new(0.17, 0, 0, 25)
    TextButton.BackgroundColor3 = Configs_HUB.Cor_Options
    TextButton.Position = position
    TextButton.Name = "ToggleButton"
    TextButton.Text = ""
    TextButton.AutoButtonColor = false

    local Frame2 = Instance.new("Frame", TextButton)
    Frame2.Size = UDim2.new(0.25, 0, 0.82, 0)
    Frame2.Position = UDim2.new(initialState and 0.8 or 0, 2, 0.5, 0)
    Frame2.AnchorPoint = Vector2.new(0, 0.5)
    Frame2.BackgroundColor3 = initialState and Color3.fromRGB(30, 140, 200) or Configs_HUB.Cor_Stroke
    Corner(Frame2, {CornerRadius = UDim.new(1, 0)})

    local OnOff = initialState

    TextButton.MouseButton1Click:Connect(function()
        OnOff = not OnOff

        if OnOff then
            CreateTween(Frame2, "Position", UDim2.new(0.8, 0, 0.5, 0), 0.2, false)
            CreateTween(Frame2, "BackgroundColor3", Color3.fromRGB(30, 140, 200), 0.2, false)
            callback(true)
            clicktoggle:Play()
        else
            CreateTween(Frame2, "Position", UDim2.new(0, 2, 0.5, 0), 0.2, false)
            CreateTween(Frame2, "BackgroundColor3", Configs_HUB.Cor_Stroke, 0.2, false)
            callback(false)
            falsetoggle:Play()
        end
    end)

    TextButton.AncestryChanged:Connect(function(_, parent)
        if not parent and OnOff then
            OnOff = false
            Frame2.Position = UDim2.new(0, 2, 0.5, 0)
            Frame2.BackgroundColor3 = Configs_HUB.Cor_Stroke
            callback(false)
        end
    end)

    return TextButton
end

functions.createSettingSection = function(parent, title, description, buttonCallback, buttonState)
    local sectionFrame = Instance.new("Frame", parent)
    sectionFrame.Size = UDim2.new(1, 0, 0, 100)
    sectionFrame.BackgroundColor3 = Configs_HUB.Cor_Hub
    sectionFrame.BackgroundTransparency = 0
    
    local titleLabel = Instance.new("TextLabel", sectionFrame)
    titleLabel.Size = UDim2.new(1, 0, 0, 30)
    titleLabel.TextScaled = true
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.BackgroundTransparency = 1
    titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleLabel.Text = title
    TextSetColor(titleLabel)
    
    local descriptionLabel = Instance.new("TextLabel", sectionFrame)
    descriptionLabel.Size = UDim2.new(1, 0, 0, 50)
    descriptionLabel.TextScaled = true
    descriptionLabel.TextXAlignment = Enum.TextXAlignment.Left
    descriptionLabel.BackgroundTransparency = 1
    descriptionLabel.Position = UDim2.new(0, 0, 0.31, 0)
    descriptionLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    descriptionLabel.Text = description
    TextSetColor(descriptionLabel)
    
    local toggleButtonPosition = buttonPosition or UDim2.new(0.82, 0, 0.75, 0)
    local toggleButton = functions.createToggleButton(sectionFrame, toggleButtonPosition, buttonCallback, buttonState)

    return sectionFrame
end

