    infoToggleButton.BackgroundColor3 = Color3.fromRGB(4, 175, 236)
    infoToggleButton.ZIndex = 10
    infoToggleButton.BackgroundTransparency = 1
    infoToggleButton.Image = "rbxthumb://type=Asset&id=101525904089827&w=150&h=150"

