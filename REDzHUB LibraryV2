  ChatToggleButton.MouseButton1Click:Connect(function()
        ChatToggle = not ChatToggle

        if ChatToggle then
            FrameChat.Visible = true
            click1:Play()
            CreateTween(FrameChat, "Size", UDim2.new(0.5, 0, 0.72, 0), 0.3, true)
            unreadCount = 0
            unreadLabel.Visible = false
        else
            click2:Play()
            CreateTween(FrameChat, "Size", UDim2.new(0.5, 0, 0, 0), 0.3, true)
            FrameChat.Visible = false
        end
    end)

   local function formatTime()
      local currentTime = os.date("*t")
      return string.format("%02d:%02d", currentTime.hour, currentTime.min)
   end

 local function output(msg, playerName, playerId)
     local messageFrame = Instance.new("Frame", LogPanel)
     messageFrame.BackgroundColor3 = Color3.new(0, 0, 0)
     messageFrame.Size = UDim2.new(1, 0, 0, 40)
     messageFrame.BorderSizePixel = 0
     messageFrame.BackgroundTransparency = 0.7
     messageFrame.LayoutOrder = LogPanel:GetChildren() and #LogPanel:GetChildren() + 1 or 1
    
     local imageplayer = Instance.new("ImageLabel", messageFrame)
     imageplayer.Size = UDim2.new(0.1, 0, 1, 0)
     imageplayer.Position = UDim2.new(0, 0, 0, 0)
     imageplayer.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
     imageplayer.BackgroundTransparency = 0.6
     imageplayer.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. playerId .. "&width=420&height=420&format=png"
     Corner(imageplayer, {CornerRadius = UDim.new(1, 0)})
    
     local messageLabel = Instance.new("TextLabel", messageFrame)
     messageLabel.Text = ":".. formatTime() .. ":" .. playerName .. ": " .. msg
     messageLabel.Size = UDim2.new(0.9, 0, 1, 0)
     messageLabel.Position = UDim2.new(0.11, 0, 0, 0)
     messageLabel.TextScaled = true
     messageLabel.TextColor3 = Configs_HUB.Cor_Text
     messageLabel.TextXAlignment = "Left"
     messageLabel.Font = Configs_HUB.Text_Font
     messageLabel.BackgroundTransparency = 1
     messageLabel.BorderSizePixel = 0
     TextSetColor(messageLabel)

       LogPanel.CanvasSize = UDim2.new(0, 0, 0, LogPanel.UIListLayout.AbsoluteContentSize.Y)
   end

  SendButton.MouseButton1Click:Connect(function()
     local message = MessageBox.Text
     if message ~= "" then
         game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
         MessageBox.Text = ""
         output(message, game.Players.LocalPlayer.Name, game.Players.LocalPlayer.UserId)
      end
   end)

local function onChatMessageReceived(player, msg)
    output(msg, player.Name, player.UserId)
    if not ChatToggle then
        unreadCount = unreadCount + 1
        unreadLabel.Text = tostring(unreadCount)
        unreadLabel.Visible = true
    end
end

local function setupPlayer(player)
    player.Chatted:Connect(function(msg)
        onChatMessageReceived(player, msg)
    end)
end

for _, player in ipairs(game.Players:GetPlayers()) do
    setupPlayer(player)
end

game.Players.PlayerAdded:Connect(setupPlayer)

