local function checkFriends()
    if friendsCooldown <= 0 then
        friendsCooldown = 25

        local playersFriends = {}
        local success, page = pcall(players.GetFriendsAsync, players, localPlayer.UserId)

        if success then
            repeat
                local info = page:GetCurrentPage()
                for _, friendInfo in pairs(info) do
                    table.insert(playersFriends, friendInfo)
                end
                local pageSuccess, _ = pcall(function() page:AdvanceToNextPageAsync() end)
                if not pageSuccess or page.IsFinished then break end
            until page.IsFinished
        end

        local friendsInTotal = #playersFriends

        local function updateFriendsGUI(friends)
            friendsFrame:ClearAllChildren()

            local positionY = 0
            for _, friend in pairs(friends) do
                local friendFrame = Instance.new("Frame")
                friendFrame.Size = UDim2.new(1, 0, 0, 50)
                friendFrame.BackgroundTransparency = 1
                friendFrame.Position = UDim2.new(0, 0, 0, positionY)
                friendFrame.Parent = friendsFrame

                local avatarImage = Instance.new("ImageLabel")
                avatarImage.Size = UDim2.new(0, 50, 0, 50)
                avatarImage.Position = UDim2.new(0, 0, 0, 0)
                avatarImage.BackgroundColor3 = Configs_HUB.Cor_Hub
                avatarImage.BackgroundTransparency = 0.4
                avatarImage.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. friend.Id .. "&width=420&height=420&format=png"
                avatarImage.Parent = friendFrame
                Corner(avatarImage, {CornerRadius = UDim.new(1, 0)})

                local friendName = Instance.new("TextLabel")
                friendName.Size = UDim2.new(0, 200, 1, 0)
                friendName.Position = UDim2.new(0, 60, 0, 0)
                friendName.BackgroundTransparency = 1
                friendName.Text = friend.Username
                friendName.TextWrapped = true
                friendName.TextColor3 = Configs_HUB.Cor_Text
                friendName.TextXAlignment = "Left"
                friendName.Font = Configs_HUB.Text_Font
                friendName.Parent = friendFrame
                TextSetColor(friendName)

                local statusLabel = Instance.new("TextLabel")
                statusLabel.Size = UDim2.new(0, 200, 1, 0)
                statusLabel.BackgroundTransparency = 1
                statusLabel.TextWrapped = true
                statusLabel.TextColor3 = Configs_HUB.Cor_Text
                statusLabel.TextXAlignment = "Left"
                statusLabel.Font = Configs_HUB.Text_Font
                statusLabel.Position = UDim2.new(0, 260, 0, 0)
                TextSetColor(statusLabel)

                if friend.IsOnline then
                    if players:FindFirstChild(friend.Username) then
                        statusLabel.Text = "In Game"
                    else
                        statusLabel.Text = "Online"
                    end
                else
                    statusLabel.Text = "Offline"
                end

                statusLabel.Parent = friendFrame
                positionY = positionY + 55
            end

            friendsFrame.CanvasSize = UDim2.new(0, 0, 0, math.max(positionY, 3000))
        end

        updateFriendsGUI(playersFriends)
        friendnumber.Text = "Friend: " .. friendsInTotal .. "/200"
        return friendsInTotal
    else
        friendsCooldown = friendsCooldown - 1
    end
end

checkFriends()

