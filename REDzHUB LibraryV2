local soundId2 = "rbxassetid://4499400560"
local notificationgui = {}

local function notify(params)
    local Title=params.Title or "Default Title"
    local Text=params.Text or "Default Message"
    local buttondeleteText=params.buttonText or "OK"
    local image=params.image or params.imageID or "18488884411"
    local guiSize=params.guiSize or Vector2.new(400,70)
    local duration=params.duration
    local position=params.Position or UDim2.new(0.5,-guiSize.X/2,0.45,50)
    local updates=params.updates
    local NotificationLabelText=Create("TextLabel",ScreenGui,{Size=UDim2.new(0,guiSize.X,0,guiSize.Y),BackgroundTransparency=1,Text=Text,TextScaled=true,TextColor3=Color3.fromRGB(255,255,255),Position=position})
    local sound2=Create("Sound",NotificationLabelText,{SoundId=soundId1,Volume=0.2,PlaybackSpeed=0.9})
    sound2:Play()
    local NotificationImage=Create("ImageLabel",NotificationLabelText,{Size=UDim2.new(0,70,0,70),Position=UDim2.new(-0.2,0,0,0),Image="rbxassetid://"..image,BackgroundTransparency=1})
    local NotificationLabelTitle=Create("TextLabel",NotificationLabelText,{Size=UDim2.new(1,0,0.4,0),BackgroundTransparency=1,Text=Title,TextScaled=true,TextColor3=Color3.fromRGB(255,255,255),Position=UDim2.new(0,0,-0.4,0)})
    local OkButton=Create("TextButton",NotificationLabelText,{Size=UDim2.new(1,0,0.4,0),Position=UDim2.new(0,0,1,0),TextColor3=Color3.fromRGB(255,255,255),TextScaled=true,Text=buttondeleteText,BackgroundTransparency=1})
    local sound1=Create("Sound",OkButton,{SoundId=soundId2,Volume=0.2,PlaybackSpeed=0.9})
    local _Title=NotificationLabelTitle.Text
    local _Text=NotificationLabelText.Text
    local _Image=NotificationImage.Image
    local ____
    if type(updates)=="table" and next(updates) then
        ____=RunService.RenderStepped:Connect(function()
            for key,fn in pairs(updates) do
                local value=(type(fn)=="function") and fn() or tostring(fn)
                if key=="Title" and value~=_Title then
                    NotificationLabelTitle.Text=value
                    _Title=value
                elseif key=="Text" and value~=_Text then
                    NotificationLabelText.Text=value
                    _Text=value
                elseif key=="Image" and value~=_Image then
                    NotificationImage.Image="rbxassetid://"..tostring(value)
                    _Image=NotificationImage.Image
                end
            end
        end)
    end
    table.insert(notificationgui,NotificationLabelText)
    if #notificationgui>1 then
        local oldNotification=table.remove(notificationgui,1)
        oldNotification:Destroy()
    end
    local tween=TweenService:Create(NotificationLabelText,TweenInfo.new(0.5),{Position=UDim2.new(position.X.Scale,position.X.Offset,position.Y.Scale,position.Y.Offset-(#notificationgui*60))})
    tween:Play()
    if duration then
        delay(duration,function()
            local tweenOut=TweenService:Create(NotificationLabelText,TweenInfo.new(0.5),{Position=UDim2.new(0.5,-guiSize.X/2,0.45,100),TextTransparency=1})
            tweenOut:Play()
            tweenOut.Completed:Connect(function()
                if updateConnection then updateConnection:Disconnect() end
                NotificationLabelText:Destroy()
            end)
        end)
    end
    OkButton.MouseButton1Click:Connect(function()
        sound1:Play()
        local tweenOut=TweenService:Create(NotificationLabelText,TweenInfo.new(0.5),{Position=UDim2.new(0.5,-guiSize.X/2,0.45,100),TextTransparency=1})
        tweenOut:Play()
        tweenOut.Completed:Connect(function()
            sound2:Destroy()
            sound1:Destroy()
            if ____ then ____:Disconnect() ____=nil end
            NotificationLabelText:Destroy()
        end)
     end)
  end
  local d={greet={"Hey, ","Yo, ","Sup! ","What‚Äôs up? ","Hello, ","Look! ","Heads up! ","Check it! ","Yo yo! ","Hiya! ","Howdy! ","Heya! ","Ahoy! ","Greetings! "},emo={"üéâ","‚ú®","üî•","üåü","üéÜ","üåà","üí™","üòä","üòé","üåô","üåû","üçÄ","üå∫","üå∏","‚òÄÔ∏è","üåõ"}}
  local function r(t) return t[math.random(#t)] end
  local greetTxt=r(d.greet)
  local emoTxt=r(d.emo)
  function getTitleText()
      local now=os.time()
      local t=os.date("*t",now)
      local y=t.year
      local nextNY=os.time{year=y+1,month=1,day=1,hour=0,min=0,sec=0}
      local diff=nextNY-now
      if diff<0 then diff=0 end
      local d=math.floor(diff/86400)
      local h=math.floor((diff%86400)/3600)
      local m=math.floor((diff%3600)/60)
      local sec=diff%60
      return string.format("%s %d:%02d:%02d:%02d Countdown to New Year!‚è∞",greetTxt,d,h,m,sec)
  end
  function getNYText()
     local now=os.time()
     local t=os.date("*t",now)
     local y=t.year
     local startNY=os.time{year=y,month=1,day=1,hour=0,min=0,sec=0}
     local endNY=os.time{year=y,month=1,day=1,hour=6,min=1,sec=0}
     if now>=startNY and now<endNY then
        return "üéâ Happy New Year "..y.."! üéÜ\nWishing you a joyful new year!"
     else
        return "Hope you enjoy using it\nWhen I update the Gui, I would like to say that it may cause the ui to not work or crash, so please understand"
     end
 end
 notify({
 Title=getTitleText(),
 Text=getNYText(),
 buttonText="Ok",
 imageID="18488884411",
 guiSize=Vector2.new(400,80),
 updates={Title=getTitleText,Text=getNYText}
 })
 function MinimizeButton(Configs)
    local functions = {} 
    local frameRefs, metaRefs, ddesug, stopupdateall, _XyZ, _cvZ = {}, {}, {}, {}, {}, {}
    local minimize1 = false
    local minimize = true
    local minimize3 = false
    local minimiz7 = true
    local PlayerList = true
    local minimize9 = false
    local player = game:FindFirstChildOfClass("Players").LocalPlayer
    local Players = game:FindFirstChildOfClass("Players")
    local players = game:FindFirstChildOfClass("Players")
    local localPlayer = game:FindFirstChildOfClass("Players").LocalPlayer
    local LocalPlayer = game:FindFirstChildOfClass("Players").LocalPlayer
    local maxPlayers = game:FindFirstChildOfClass("Players").MaxPlayers
    local PlaceId = game.PlaceId
    local friendsCooldown = 0
    local friendsInTotal = 0
    local MarketplaceService = game:GetService("MarketplaceService")
    local IconImageAssetId = MarketplaceService:GetProductInfo(PlaceId).IconImageAssetId
    local ContentProvider = game:GetService("ContentProvider")
    local getgame = MarketplaceService:GetProductInfo(PlaceId)
    local gameName = getgame and getgame.Name or "Unknown"
    local AssetService = game:GetService("AssetService")
    local TeleportService = game:GetService("TeleportService")
    local GetGamepass = MarketplaceService:GetDeveloperProductsAsync():GetCurrentPage()
    local GetSplitGame = AssetService:GetGamePlacesAsync():GetCurrentPage()
    local count, jt = 1, os.time()
    local Player = players.LocalPlayer
    local Mouse = LocalPlayer:GetMouse()
    local HttpService, HttpGet = game:GetService("HttpService"), game.HttpGet
    local View, oldicon = nil, nil
    local playerFrames = {}
    local displayedFriends = {}
    local _z9, _p, _q, _s = _z9 or {}, _p or {}, _q or {}, _s or {}
    local infogamepass = {}
    local frameinfopy = false
    local Avatar = false
    local switchtodescription = false
    local waitswitchpage = false
    local image = Configs.Image or ""
    text1_1 = ""
    text1_2 = "\nFollowersüîî:Loading...\nFollowingsüë•:Loading..."
    functions.RunServerlock = function(lockName, Time, callback)
        if stopupdateall[lockName] then
           stopupdateall[lockName]:Disconnect()
       end
        if Time == "no" or Time == 0 then
           stopupdateall[lockName] = RunService.Heartbeat:Connect(callback)
        else
           local Updates = tick()
           stopupdateall[lockName] = RunService.Heartbeat:Connect(function()
               if tick() - Updates >= Time then
                  Updates = tick()
                  callback()
              end
           end)
       end
   end
   RunServerlock = functions.RunServerlock
   functions.formatNumber = function(n)
     local suffixes = {
        {s = "Vg", v = 1e63}, {s = "Nd", v = 1e60}, {s = "Od", v = 1e57},
        {s = "Spd", v = 1e54}, {s = "Sxd", v = 1e51}, {s = "Qid", v = 1e48},
        {s = "Qad", v = 1e45}, {s = "Td",  v = 1e42}, {s = "Dd",  v = 1e39},
        {s = "Ud",  v = 1e36}, {s = "Dc",  v = 1e33}, {s = "No",  v = 1e30},
        {s = "Oc",  v = 1e27}, {s = "Sp",  v = 1e24}, {s = "Sx",  v = 1e21},
        {s = "Qi",  v = 1e18}, {s = "Qa",  v = 1e15}, {s = "T",   v = 1e12},
        {s = "B",   v = 1e9},  {s = "M",   v = 1e6},  {s = "k",   v = 1e3}
    }
    for _, e in ipairs(suffixes) do
        if n >= e.v then
            local val = n / e.v
            local rounded = math.floor(val * 10 + 0.5) / 10
            return (rounded == math.floor(rounded)) and (string.format("%d%s", rounded, e.s)) or (string.format("%.1f%s", rounded, e.s))
        end
    end
    return tostring(n)
  end
 formatNumber = functions.formatNumber

