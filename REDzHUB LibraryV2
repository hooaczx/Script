functions.UpdateDescription = function(params)
    local c_1 = params.fromFrame
    local c_2 = params.Text
    local c_3 = params.description
    local c_4 = params.image
    local c_5 = params.textwrapped or false
    local c_6 = params.textscaled or false
    local c_7 = params.size or 8    
    local c_8 = frameRefs[c_1]
    local c_9 = metaRefs[c_1]
    if c_9 and c_9.Text == c_2 and c_9.Description == c_3 and c_9.Image == c_4 then
        return
    end
    if c_8 and c_8.Parent then
        for _, child in ipairs(c_8:GetChildren()) do
            if child:IsA("TextLabel") then
                -- ‡πÅ‡∏¢‡∏Å title ‡∏Å‡∏±‡∏ö description ‡πÇ‡∏î‡∏¢‡∏î‡∏π‡∏Ç‡∏ô‡∏≤‡∏î‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á (‡∏ï‡∏≤‡∏°‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö‡πÄ‡∏î‡∏¥‡∏°)
                local yScale = child.Size and child.Size.Y and child.Size.Y.Scale or 0
                if yScale >= 0.5 then
                    -- description (‡πÉ‡∏´‡∏ç‡πà)
                    child.Text = c_3 or ""
                    child.TextWrapped = c_5
                    child.TextScaled = c_6
                    child.TextSize = c_7
                    if typeof(TextSetColor) == "function" then
                        pcall(TextSetColor, child)
                    end
                else
                    child.Text = c_2 or ""
                    if typeof(TextSetColor) == "function" then
                        pcall(TextSetColor, child)
                    end
                end
            elseif child:IsA("ImageLabel") then
                child.Image = c_4 or ""
            end
        end
        metaRefs[c_1] = {
            Text = c_2,
            Description = c_3,
            Image = c_4
        }
        return c_8
    end
    local c_10 = Create("Frame", descriptionframe, {
        Size = UDim2.new(0.8, 0, 0.4, 0),
        BackgroundColor3 = Color3.fromRGB(0, 0, 0),
        BackgroundTransparency = 0.5
    })
    local c_11 = Create("TextLabel", c_10, {
        Size = UDim2.new(0.88, 0, 0.1, 0),
        Position = UDim2.new(0.12, 0, 0.03, 0),
        Text = c_2 or "",
        TextColor3 = Configs_HUB.Cor_Text,
        Font = Configs_HUB.Text_Font,
        BackgroundColor3 = Configs_HUB.Cor_Hub,
        BackgroundTransparency = 1
    })TextSetColor(c_11)
    local c_12 = Create("TextLabel", c_10, {
        Size = UDim2.new(1, 0, 0.84, 0),
        Position = UDim2.new(0, 0, 0.16, 0),
        Text = c_3 or "",
        TextColor3 = Configs_HUB.Cor_Text,
        Font = Configs_HUB.Text_Font,
        BackgroundColor3 = Configs_HUB.Cor_Hub,
        BackgroundTransparency = 1,
        TextWrapped = c_5,
        TextScaled = c_6,
        TextSize = c_7,
        TextXAlignment = Enum.TextXAlignment.Left,
        TextYAlignment = Enum.TextYAlignment.Top
    })TextSetColor(c_12)
    Create("Frame", c_10, {
        Size = UDim2.new(1, 0, 0.005, 0),
        Position = UDim2.new(0, 0, 0.16, 0),
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BorderSizePixel = 0
    })
    Create("ImageLabel", c_10, {
        Size = UDim2.new(0.11, 0, 0.16, 0),
        Position = UDim2.new(0, 0, 0, 0),
        AnchorPoint = Vector2.new(0.05, 0.06),
        BackgroundColor3 = Configs_HUB.Cor_Hub,
        BackgroundTransparency = 1,
        Image = c_4 or ""
    })
    frameRefs[c_1] = c_10
    metaRefs[c_1] = {
        Text = c_2,
        Description = c_3,
        Image = c_4
    }
    return c_10
end

functions.getplayerdescription = function(userId)
    local zx_Desc = ddesug[userId]
    if zx_Desc and zx_Desc ~= "" then
        return zx_Desc
    end
    local HttpService = game:GetService("HttpService")
    local success, response = pcall(function()
        return game:HttpGet("https://users.roblox.com/v1/users/" .. userId)
    end)
    if not success or not response then
        return zx_Desc or "Failed to load description"
    end
    local ok, data = pcall(function()
        return HttpService:JSONDecode(response)
    end)
    local desc = ""
    if ok and data then
        desc = data.description or ""
    end
    if desc == "" then
        if userId == game:FindFirstChildOfClass("Players").LocalPlayer.UserId then
            desc = "No description set for your account."
        else
            desc = "User has no description."
        end
    end
    if zx_Desc == desc then
        return zx_Desc
    end
    ddesug[userId] = desc
    return desc
end

functions.getGameDesc = function(_id)
    local cachedDesc = _XyZ[_id]
    if cachedDesc and cachedDesc ~= "" then
        return cachedDesc
    end
    local MarketplaceService = game:GetService("MarketplaceService")
    local success, productInfo = pcall(function()
        return MarketplaceService:GetProductInfo(_id)
    end)
    if not success or not productInfo then
        return cachedDesc or "Failed to load game description"
    end
    local desc = productInfo.Description or "This game has no description"
    if cachedDesc == desc then
        return cachedDesc
    end
    _XyZ[_id] = desc
    return desc
end

functions.startUpdating = function()
    functions.__ = (functions.__ or 0) + 1
    local _a = game:FindFirstChildOfClass("Players").LocalPlayer.AccountAge
    local _b = _a * 86400 + functions.__
    local _c = math.floor(_b / 31536000)
    _b = _b % 31536000
    local _d = math.floor(_b / 2592000)
    _b = _b % 2592000
    local _e = math.floor(_b / 86400)
    local _f = string.format("%d years, %d months, %d days", _c, _d, _e)
    local _g = _a == 0 and "You just created an account"
        or string.format("Your account has been active for %d days", _a)
    AccountAge.Text = "Total Playtime since account creation: " .. _f .. "\n" .. _g
end

functions.startUpdating()

functions.updatePlaytime = function()
    TimePlay.Text = "Time Play: " .. (function(pt)
        if pt < 60 then return string.format("%02d", pt)
        elseif pt < 3600 then return string.format("%d:%02d", math.floor(pt/60), pt%60)
        elseif pt < 86400 then return string.format("%d:%02d:%02d", math.floor(pt/3600), math.floor((pt%3600)/60), pt%60)
        elseif pt < 2592000 then return string.format("%d:%02d:%02d:%02d", math.floor(pt/86400), math.floor((pt%86400)/3600), math.floor((pt%3600)/60), pt%60)
        elseif pt < 31536000 then return string.format("%d:%02d:%02d:%02d:%02d", math.floor(pt/2592000), math.floor((pt%2592000)/86400), math.floor((pt%86400)/3600), math.floor((pt%3600)/60), pt%60)
        else return string.format("%d:%02d:%02d:%02d:%02d:%02d", math.floor(pt/31536000), math.floor((pt%31536000)/2592000), math.floor((pt%2592000)/86400), math.floor((pt%86400)/3600), math.floor((pt%3600)/60), pt%60)
        end
    end)(os.time() - jt) .. "\nThis shows how long you've been playing"
end

functions.updatePlaytime()

functions.getAccountCreationDate = function(ageInDays)
    local age = os.date("*t", os.time() - (ageInDays * 86400))
    return string.format("%02d/%02d/%d", age.month, age.day, age.year)
end

function gf(u)
    local f, fl, c1, c2 = {}, {}, "", ""
    repeat
        local ok, response = pcall(function()
            return game:HttpGet("https://friends.roblox.com/v1/users/"..u.."/followers?limit=100&cursor="..c1)
        end)
        if not ok or not response then
            return "Error_Followers", "Error_Followings"
        end
        local ok2, data = pcall(function()
            return game:GetService("HttpService"):JSONDecode(response)
        end)
        if not ok2 or not data or not data.data then
            return "Error_Followers", "Error_Followings"
        end
        for i = 1, #data.data do
            f[#f+1] = data.data[i]
        end
        c1 = data.nextPageCursor or ""
    until c1 == ""
    repeat
        local ok, response = pcall(function()
            return game:HttpGet("https://friends.roblox.com/v1/users/"..u.."/followings?limit=100&cursor="..c2)
        end)
        if not ok or not response then
            return "Error_Followers", "Error_Followings"
        end
        local ok2, data = pcall(function()
            return game:GetService("HttpService"):JSONDecode(response)
        end)
        if not ok2 or not data or not data.data then
            return "Error_Followers", "Error_Followings"
        end
        for i = 1, #data.data do
            fl[#fl+1] = data.data[i]
        end
        c2 = data.nextPageCursor or ""
    until c2 == ""
    return f, fl
end

functions.updateTime = function()
	local nw = os.time()
	local x = os.date("*t", nw)
	local h, m, s = x.hour, x.min, x.sec
	local ap = "AM"
	local hh = h
	if h >= 12 then
		ap = "PM"
		if h > 12 then
			hh = h - 12
		end
	elseif h == 0 then
		hh = 12
	end
	local timeStr = string.format("%02d:%02d:%02d %s", hh, m, s, ap)
	local dateStr = os.date("%b:%d:%Y", nw)
	TimeLabelgui.Text = timeStr .. "\n" .. dateStr
end

functions.updateTime()

