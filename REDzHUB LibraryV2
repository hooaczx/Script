local buttonCooldown, Click, ifClick, specialClicks = 0.5, 0, false, {}
functions.canClick = function(buttonName)
    local Time = tick()
    if buttonName then
        if not specialClicks[buttonName] or Time - specialClicks[buttonName] >= buttonCooldown then
            specialClicks[buttonName] = Time
            return true
        end
        return false
    else
        if Time - Click >= buttonCooldown and not ifClick then
            Click = Time
            ifClick = true
            delay(buttonCooldown, function()
                ifClick = false
            end)
            return true
        end
        return false
    end
end
functions.Look = function()
  local c = workspace.CurrentCamera
  local d = c.CFrame.LookVector
  local lp = game:FindFirstChildOfClass("Players").LocalPlayer
  local r = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
  if not r then return c.CFrame.Position + d * 100 end
  local v = Vector3.new(d.X, 0, d.Z).Unit
  return r.Position + v
end

ShiftLockButton.MouseButton1Click:Connect(function()
    if not functions.canClick("Free ShiftLocküòå") or Tweening then return end
    Tweening = true
    if not ShiftLockmaingui then
        local ZX_r = {}
        local ZX_highlighted = nil
        CH = {}
        ShiftLockmaingui = RunService.RenderStepped:Connect(function()
            local ZX_chr = Player.Character or Player.CharacterAdded:Wait()
            if not ZX_chr then return end
            local ZX_rp = ZX_chr:FindFirstChild("HumanoidRootPart")
            local ZX_h = ZX_chr:FindFirstChildOfClass("Humanoid")
            local ZX_cam = workspace.CurrentCamera
            if not (ZX_rp and ZX_h and ZX_cam) then return end
            ZX_h.AutoRotate = false
            ShiftLockButton.Image = States.On
            ShiftlockCursor.Visible = true
            local ZX_look = functions.Look()
            local ZX_pos = ZX_rp.Position
            ZX_look = Vector3.new(ZX_look.X, ZX_pos.Y, ZX_look.Z)
            ZX_rp.CFrame = CFrame.new(ZX_pos, ZX_look)
            CreateTween(ZX_h, "CameraOffset", Vector3.new(1.9, 0, 0), 0.5, false)
            local ZX_plrs = game:FindFirstChildOfClass("Players")
            local ZX_lp = ZX_plrs.LocalPlayer
            local ZX_all = ZX_plrs:GetPlayers()
            local ZX_cp = ZX_cam.CFrame.Position
            if #ZX_all == 1 and ZX_lp.Name == ZX_all[1].Name then
                DisplayNameplayers.Visible = false
                Nameplayers.Visible = false
                useridplayers.Visible = false
                for _, ZX_hi in pairs(CH) do if ZX_hi then ZX_hi:Destroy() end end
                CH = {}
                imageplayers.Image = "rbxthumb://type=Asset&id=100283719478427&w=150&h=150"
                noplayers.Visible = true
                Tweening = false
                return
            else
                DisplayNameplayers.Visible = true
                Nameplayers.Visible = true
                useridplayers.Visible = true
                noplayers.Visible = false
            end
            local ZX_ctr = ShiftlockCursor.AbsolutePosition + (ShiftlockCursor.AbsoluteSize / 2)
            local ZX_sz = ShiftlockCursor.AbsoluteSize
            local ZX_dir = ZX_cam.CFrame.LookVector
            local ZX_cd = {}
            for i = 1, #ZX_all do
                local pl = ZX_all[i]
                if pl ~= ZX_lp then
                    local ch = pl.Character
                    if ch and ch.Parent == workspace then
                        if not ZX_r[pl] or ZX_r[pl].char ~= ch then
                            local t = {}
                            local m = {}
                            local desc = ch:GetDescendants()
                            for d = 1, #desc do
                                local p = desc[d]
                                if p and p:IsA("BasePart") and p.Transparency < 1 then
                                    t[#t + 1] = p
                                    m[p] = true
                                end
                            end
                            ZX_r[pl] = { char = ch, parts = t, pt = m, n = pl.Name, d = pl.DisplayName, u = pl.UserId }
                        else
                            local zx = ZX_r[pl]
                            if zx and zx.parts then
                                local tt = zx.parts
                                local ni = 1
                                for k = 1, #tt do
                                    local p = tt[k]
                                    if p and p.Parent and p:IsA("BasePart") and p.Transparency < 1 then
                                        tt[ni] = p
                                        ni = ni + 1
                                    else
                                        if zx.pt then zx.pt[p] = nil end
                                    end
                                end
                                for k = ni, #tt do tt[k] = nil end
                            end
                        end
                        local zx = ZX_r[pl]
                        if zx and zx.parts then
                            local ZX_b = math.huge
                            local ZX_bp = nil
                            for j = 1, #zx.parts do
                                local p = zx.parts[j]
                                if p and p.Parent and p:IsA("BasePart") and p.Transparency < 1 then
                                    local spos, on = ZX_cam:WorldToScreenPoint(p.Position)
                                    if on then
                                        local inX = spos.X >= ZX_ctr.X - ZX_sz.X / 2 and spos.X <= ZX_ctr.X + ZX_sz.X / 2
                                        local inY = spos.Y >= ZX_ctr.Y - ZX_sz.Y / 2 and spos.Y <= ZX_ctr.Y + ZX_sz.Y / 2
                                        if inX and inY then
                                            local d2 = (Vector2.new(spos.X, spos.Y) - ZX_ctr).Magnitude
                                            local f = (p.Position - ZX_cp):Dot(ZX_dir)
                                            if f > 0 and d2 < ZX_b then
                                                ZX_b = d2
                                                ZX_bp = p
                                            end
                                        end
                                    end
                                end
                            end
                            if ZX_bp then ZX_cd[#ZX_cd + 1] = { plr = pl, part = ZX_bp, d2 = ZX_b } end
                        end
                    else
                        ZX_r[pl] = nil
                    end
                end
            end
            if #ZX_cd > 0 then
                table.sort(ZX_cd, function(a, b) return a.d2 < b.d2 end)
                local ZX_near = ZX_cd[1].plr
                if ZX_near then
                    if not game.Players:FindFirstChild(ZX_near.Name) or not ZX_near.Character then ZX_near = nil end
                end
                if ZX_near then
                    local ZX_last = ZX_r[ZX_near]
                    local ZX_namechg = not ZX_last or ZX_last.n ~= ZX_near.Name or ZX_last.d ~= ZX_near.DisplayName
                    local ZX_partsChanged = false
                    if ZX_last and ZX_last.char then
                        for _, p in pairs(ZX_near.Character:GetDescendants()) do
                            if p:IsA("BasePart") and p.Transparency < 1 and not (ZX_last.pt and ZX_last.pt[p]) then
                                ZX_partsChanged = true
                                break
                            end
                        end
                    end
                    if not ZX_last or ZX_namechg or ZX_partsChanged or not CH[ZX_near] then
                        DisplayNameplayers.Text = ZX_near.DisplayName
                        Nameplayers.Text = "@" .. ZX_near.Name
                        useridplayers.Text = "id:" .. ZX_near.UserId
                        imageplayers.Image = "http://www.roblox.com/Thumbs/Avatar.ashx?x=500&y=500&Format=Png&userId=" .. ZX_near.UserId
                        if CH[ZX_near] then CH[ZX_near]:Destroy() end
                        if frameinfopy then
                            local ZX_hi = Create("Highlight", ZX_near.Character, { Name = "ZX_PHl_hooaczx", Adornee = ZX_near.Character, FillTransparency = 1, OutlineTransparency = 0, DepthMode = Enum.HighlightDepthMode.AlwaysOnTop, OutlineColor = Color3.fromRGB(0, 0, 255) })
                            CH[ZX_near] = ZX_hi
                        end
                        local ZX_pt = {}
                        for _, p in pairs(ZX_near.Character:GetDescendants()) do
                            if p:IsA("BasePart") and p.Transparency < 1 then ZX_pt[p] = true end
                        end
                        ZX_r[ZX_near] = ZX_r[ZX_near] or {}
                        ZX_r[ZX_near].pt = ZX_pt
                        ZX_r[ZX_near].n = ZX_near.Name
                        ZX_r[ZX_near].d = ZX_near.DisplayName
                        ZX_r[ZX_near].u = ZX_near.UserId
                    end
                    ZX_highlighted = ZX_near
                end
            else
                if ZX_highlighted then
                    if CH[ZX_highlighted] then CH[ZX_highlighted]:Destroy() CH[ZX_highlighted] = nil end
                    ZX_highlighted = nil
                end
                DisplayNameplayers.Text = "nil"
                Nameplayers.Text = "@nil"
                useridplayers.Text = "id:nil"
                imageplayers.Image = "rbxthumb://type=Asset&id=100283719478427&w=150&h=150"
            end
            Tweening = false
        end)
    else
        local ZX_chr3 = Player.Character
        if ZX_chr3 then
            local ZX_h3 = ZX_chr3:FindFirstChildOfClass("Humanoid")
            if ZX_h3 then ZX_h3.AutoRotate = true end
        end
        for _, ZX_hi in pairs(CH) do if ZX_hi then ZX_hi:Destroy() end end
        CH = {}
        ZX_highlighted = nil
        ZX_r = {}
        ShiftLockButton.Image = States.Off
        ShiftlockCursor.Visible = false
        if ShiftLockmaingui then ShiftLockmaingui:Disconnect() ShiftLockmaingui = nil end
        local ZX_hf = game:FindFirstChildOfClass("Players").LocalPlayer.Character or game:FindFirstChildOfClass("Players").LocalPlayer.CharacterAdded:Wait()
        CreateTween(ZX_hf.Humanoid, "CameraOffset", Vector3.new(0, 0, 0), 0.5, true)
        Tweening = false
    end
end)

local uiStates = {
    framek = {frame = framek, state = false},
    framep = {frame = framep, state = false},
    frameinfogui = {frame = frameinfogui, state = false},
    Menu = {frame = Menu, state = true},
    frameSetting = {frame = frameSetting, state = false}
}local activeButton, cameraback = nil, true

local strokeeffect = Create("Frame", frame, {
    Size = UDim2.new(0.06, 0, 0.6, 0),
    Position = UDim2.new(0.22, 0, 0.22, 0),
    BackgroundTransparency = 1,
    ZIndex = 10
})

local stroke = Create("UIStroke", strokeeffect, {
    Color = Color3.fromRGB(255, 255, 255),
    ApplyStrokeMode = Enum.ApplyStrokeMode.Border,
    Transparency = 0
})

functions.MoveStrokePosition = function(button)
    CreateTween(strokeeffect, "Position", button.Position, 0.2, true)
    activeButton = button.Name
end

functions.bouncefov = function(action, string)
   CreateTween(workspace.CurrentCamera, "FieldOfView", 75, 0.07, true)
    task.wait(0.1)
   CreateTween(workspace.CurrentCamera, "FieldOfView", 30, 0.07, true)
end
functions.Backfov = function(action, string)
   CreateTween(workspace.CurrentCamera, "FieldOfView", 70, 0.07, true)
end

functions.closeOtherUI = function(exceptName)
    for name, data in pairs(uiStates) do
        if name ~= exceptName and data.state then
            if name == "Menu" then
                if IsMinimizedu then
                    data.state = false
                    CreateTween(Menu, "Size", UDim2.new(0.576, 0, 0, 0), 0.15, true)
                    data.frame.Visible = false
                    Menu.Position = UDim2.new(0.77, 0, 0.227, 0)
                else
                    data.state = false
                    CreateTween(data.frame, "Size", UDim2.new(data.frame.Size.X.Scale, 0, 0, 0), 0.3, true)
                    data.frame.Visible = false
                    Menu.Position = UDim2.new(0.77, 0, 0.227, 0)
                end
            elseif name == "frameinfogui" then
               if not cameraback then
                 data.state = false
                 if blur.Visible then
                    CreateTween(blur, "BackgroundTransparency", 1, 0.05, true)
                    task.delay(0.05, function()
                        blur.Visible = false
                      end)
                 end
                 functions.Backfov()
                 CreateTween(data.frame, "Size", UDim2.new(data.frame.Size.X.Scale, 0, 0, 0), 0.3, true)
                 task.delay(0.3, function()
                    data.frame.Visible = false
                 end)
                 cameraback = true
                end
            else
                data.state = false
                CreateTween(data.frame, "Size", UDim2.new(data.frame.Size.X.Scale, 0, 0, 0), 0.3, true)
                data.frame.Visible = false
            end
        end
    end
end

SplitgameToggleButton.MouseButton1Click:Connect(function()
    if switchtodescription or ifClick or (switchtodescription and ifClick) then return end
    if not functions.canClick() then return end
    functions.MoveStrokePosition(SplitgameToggleButton)
    local state = uiStates.framek.state
    if not state then
        functions.closeOtherUI("framek")
        click1:Play()
        framek.Visible = true
        CreateTween(framek, "Size", UDim2.new(0.7, 0, 0.64, 0), 0.3, true)
    else
        click2:Play()
        CreateTween(framek, "Size", UDim2.new(0.7, 0, 0, 0), 0.3, true)
        framek.Visible = false
    end
    uiStates.framek.state = not state
end)

