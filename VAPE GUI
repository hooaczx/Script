loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/hooaczx/Script/main/Vape"))()
loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/hooaczx/Script/main/vape"))()

task.spawn(function()
    local teleportConnection = game.Players.LocalPlayer.OnTeleport:Connect(function(State)
        if (not teleportedServers) then
    local queueonteleport = syn and syn.queue_on_teleport or queue_on_teleport or function() end
        queueonteleport([[
            if not game:IsLoaded() then
                game.Loaded:Wait()
            end
            loadstring(game:HttpGet("https://raw.githubusercontent.com/hooaczx/Script/main/Vape%20gui"))();
        ]])
    end
    end)
end)
