-- Generate Random IP
local function generateRandomIP()
    local part1 = math.random(10, 99)
    local part2 = math.random(0, 9)
    local part3 = math.random(10, 99)
    return part1 .. "." .. part2 .. "." .. part3 .. ".##"
end

