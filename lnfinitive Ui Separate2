    Name = "Take Screenshot",
    Callback = function()
        game:GetService("CoreGui"):TakeScreenshot()
        OrionLib:MakeNotification({
            Name = "Screenshot Taken",
            Content = "A screenshot has been successfully taken.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end    
})

-- Initialize the GUI
